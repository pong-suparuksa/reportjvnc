	<parameter name="SELECT_MONTH" class="java.lang.String">
		<defaultValueExpression><![CDATA["03/2020"]]></defaultValueExpression>
	</parameter>
	<queryString language="SQL">
		<![CDATA[SELECT LTC.TITLE_CASE_NAME || C.BLACK_IDNUM || '/' || C.BLACK_YEAR  BLACK_CASE_NO,
LB.BANK_NAME, BSB.BOND_NO,
THAIMONTH(EXTRACT(MONTH FROM B.BAIL_DATE)) MONTH_STR,
THAIMONTH(EXTRACT(YEAR FROM B.BAIL_DATE) +543) YEAR_STR, THAISDATE(B.BAIL_DATE) THAIDATE,	
LT.TITLE_NAME || BB.NAME || ' ' || BB.LASTNAME BAILMAN_NAME,
BSB.VALUE_AMOUNT, COU.COURT_NAME_TH,
CASE BSB.BOND_TYPE_ID
	WHEN 1 THEN 'พันธบัตร'
    WHEN 2 THEN 'สลากออมสิน'
    ELSE 'อื่น ๆ'
END BOND_TYPE

FROM PC_BAIL_SEC_BOND BSB
LEFT JOIN PC_BAIL_SEC BS ON(BS.SEC_ID=BSB.SEC_ID AND BS.REMOVED_BY = 0)
LEFT JOIN PC_BAIL_BAILSMAN BB ON(BS.BAILSMAN_ID=BB.BAILSMAN_ID AND BB.REMOVED_BY = 0)
LEFT JOIN PC_BAIL B ON(B.BAIL_ID=BB.BAIL_ID AND B.REMOVED_BY = 0)
--
LEFT JOIN PC_CASE C ON(C.CASE_ID=B.CASE_ID AND C.REMOVED_BY = 0)
LEFT JOIN PC_LOOKUP_TITLE_CASE LTC ON(LTC.TITLE_CASE_ID=C.BLACK_TITLE_ID AND LTC.REMOVED_BY = 0)
LEFT JOIN (SELECT plc.COURT_NAME_TH
			FROM PC_PARAM pp
			JOIN PC_LOOKUP_COURT plc ON plc.COURT_ID = pp.PARAM_VALUE AND plc.REMOVED_BY = 0
			WHERE pp.PARAM_NAME = 'COURT_ID' AND pp.REMOVED_BY = 0) COU ON 1 = 1
LEFT JOIN PC_LOOKUP_BANK LB ON(LB.BANK_ID=BSB.BANK_ID)
--
LEFT JOIN PC_LOOKUP_TITLE LT ON(LT.TITLE_ID=BB.NAME_TITLE_ID AND LT.REMOVED_BY = 0)
LEFT JOIN PC_BAIL_BOND_TYPE LBT ON(LBT.BOND_TYPE_ID=BSB.BOND_TYPE_ID AND LBT.REMOVED_BY = 0)
--
WHERE  BSB.BOND_TYPE_ID = 1 AND BSB.REMOVED_BY = 0 
AND TO_DATE(TO_CHAR(B.BAIL_DATE, 'MM-YYYY'),'MM-YYYY') = TO_DATE($P{SELECT_MONTH}, 'MM-YYYY')]]>
	</queryString>
