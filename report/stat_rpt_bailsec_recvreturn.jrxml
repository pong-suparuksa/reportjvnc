--
LEFT JOIN (SELECT pbsa.SEC_ID,  
			       plb.BANK_NAME||' สาขา'||pbsa.BANK_BRANCH SEC_DESC, 
			       pbsa.BALANCE_AMOUNT   SEC_AMOUNT
			FROM PC_BAIL_SEC_ACCOUNT pbsa 
