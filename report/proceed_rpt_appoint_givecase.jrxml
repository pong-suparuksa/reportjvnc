	<queryString language="SQL">
		<![CDATA[SELECT DISTINCT
       ppac.START_DATE,
       THAILDATE(ppac.START_DATE,1,0,0) APPOINT_DATE,
       SUBSTR(ppac.CHOICE_TIME,1,5) APPOINT_TIME,
       NVL(pltc.TITLE_CASE_NAME,' ')  || TO_CHAR(pj.RED_IDNUM) || CASE WHEN pj.RED_IDNUM IS NOT NULL THEN  '/' END || TO_CHAR(pj.RED_YEAR)     CASE_RED_NO,
       NVL(cl.LITIGANT_NAME,' ')   CLAIMANT,
       NVL(de.LITIGANT_NAME,' ')   DEFENDANT,
       CASE WHEN pb.CASE_ID IS NOT NULL THEN 'ประกัน' ELSE ' ' END BAIL,
       NVL(pc.ALLE_DESC,' ')   TOPIC,
       plar.APPOINT_REASON_NAME  APPOINT_REASON,
       plac.APPOINT_CASE_NAME,
       NVL(lyc.LITIGANT_NAME,' ')  CLAIMANT_PART,
       NVL(lyd.LITIGANT_NAME,' ')  DEFENDANT_PART,
       jud.JUDGE_NAME,
       NVL(pljr.JUDGE_ROOM_NAME,' ') ROOM
--
FROM PC_PROCEED_APPOINT ppa 
JOIN PC_PROCEED_APPOINT_CONTINUE ppac ON ppac.APPOINT_ID = ppa.APPOINT_ID 
--JOIN (SELECT ppac.APPOINT_ID, MIN(ppac.APPOINT_CON_ID) APPOINT_CON_ID 
--	  FROM PC_PROCEED_APPOINT_CONTINUE ppac 
--	  GROUP BY ppac.APPOINT_ID) fapp ON fapp.APPOINT_CON_ID = ppac.APPOINT_CON_ID 
--
LEFT JOIN PC_JUDGEMENT pj ON pj.CASE_ID = ppa.CASE_ID AND pj.REMOVED_BY = 0
LEFT JOIN PC_LOOKUP_TITLE_CASE pltc ON pltc.TITLE_CASE_ID = pj.RED_TITLE_ID AND pltc.REMOVED_BY = 0
--โจทก์
LEFT JOIN (SELECT pcl.CASE_ID, pcl.LITIGANT_NAME 
			FROM PC_CASE_LIT pcl 
			JOIN PC_LOOKUP_LIT_TYPE pllt ON pllt.LIT_TYPE_ID = pcl.LITIGANT_TYPE_ID 
			                             AND pllt.REMOVED_BY = 0
			WHERE pllt.LIT_TYPE_CODE = '01') cl ON cl.CASE_ID = ppa.CASE_ID
--จำเลย
LEFT JOIN (SELECT pcl.CASE_ID, pcl.LITIGANT_NAME 
			FROM PC_CASE_LIT pcl 
			JOIN PC_LOOKUP_LIT_TYPE pllt ON pllt.LIT_TYPE_ID = pcl.LITIGANT_TYPE_ID 
			                             AND pllt.REMOVED_BY = 0
			WHERE pllt.LIT_TYPE_CODE = '03') de ON de.CASE_ID = ppa.CASE_ID
--ฝ่ายโจทก์ (ทนายความ?)
LEFT JOIN (SELECT pcl.CASE_ID, pcl.LITIGANT_NAME 
			FROM PC_CASE_LIT pcl 
			JOIN PC_LOOKUP_LIT_TYPE pllt ON pllt.LIT_TYPE_ID = pcl.LITIGANT_TYPE_ID 
			                             AND pllt.REMOVED_BY = 0
			WHERE pllt.LIT_TYPE_CODE = '16') lyc ON lyc.CASE_ID = ppa.CASE_ID
--ฝ่ายจำเลย (ทนายความ)
LEFT JOIN (SELECT pcl.CASE_ID, pcl.LITIGANT_NAME 
			FROM PC_CASE_LIT pcl 
			JOIN PC_LOOKUP_LIT_TYPE pllt ON pllt.LIT_TYPE_ID = pcl.LITIGANT_TYPE_ID 
			                             AND pllt.REMOVED_BY = 0
			WHERE pllt.LIT_TYPE_CODE = '17') lyd ON lyd.CASE_ID = ppa.CASE_ID
--
LEFT JOIN PC_BAIL pb ON pb.CASE_ID = ppa.CASE_ID AND pb.REMOVED_BY = 0
LEFT JOIN PC_CASE pc ON pc.CASE_ID = ppa.CASE_ID AND ppa.REMOVED_BY = 0
--
LEFT JOIN PC_LOOKUP_APPOINT_REASON plar ON plar.APPOINT_REASON_ID = ppa.REASON_APPOINT_ID AND plar.REMOVED_BY = 0
LEFT JOIN PC_LOOKUP_APPOINT_CASE plac ON plac.APPOINT_CASE_ID = ppa.APPOINT_BY_ID AND plac.REMOVED_BY = 0
--ห้องพิจารณา
LEFT JOIN PC_LOOKUP_JUDGE_ROOM pljr ON pljr.JUDGE_ROOM_ID = ppa.ROOM_ID AND pljr.REMOVED_BY = 0
--เจ้าของสำนวน
LEFT JOIN (SELECT pcj.CASE_ID, plj.JUDGE_NAME
            FROM PC_CASE_JUDGE pcj
            JOIN PC_LOOKUP_JUDGE plj ON plj.JUDGE_ID = pcj.JUDGE_ID
            JOIN PC_LOOKUP_JUDGE_TYPE pljt ON pljt.JUDGE_TYPE_ID = pcj.JUDGE_TYPE_ID 
                                          AND pljt.JUDGE_TYPE_CODE = '01'
                                          AND pljt.REMOVED_BY = 0
            WHERE pcj.REMOVED_BY = 0) jud ON jud.CASE_ID = ppa.CASE_ID
--
WHERE ppac.START_DATE BETWEEN TO_DATE( $P{START_DATE}||'T00:00:00', 'DD/MM/YYYY"T"HH24:MI:SS') AND TO_DATE($P{END_DATE}||'T23:59:59', 'DD/MM/YYYY"T"HH24:MI:SS')
--
AND ppac.CHOICE LIKE $P{PERIOD}
AND (ppa.APPOINT_BY_ID = $P!{APPOINT_BY_ID} OR 0 = $P!{APPOINT_BY_ID}) 
--
ORDER BY ppac.START_DATE]]>
