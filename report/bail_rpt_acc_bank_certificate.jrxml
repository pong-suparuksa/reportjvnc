	<parameter name="SELECT_MONTH" class="java.lang.String">
		<defaultValueExpression><![CDATA["03/2020"]]></defaultValueExpression>
	</parameter>
	<queryString language="SQL">
		<![CDATA[SELECT 
cou.COURT_NAME_TH,
THAISDATE(pb.BAIL_DATE) THAIDATE ,
THAIMONTH(EXTRACT(MONTH FROM pb.BAIL_DATE)) MONTH_STR,
THAIMONTH(EXTRACT(YEAR FROM pb.BAIL_DATE) +543) YEAR_STR,
CASE WHEN pc.BLACK_IDNUM IS NOT NULL THEN pltc_b.TITLE_CASE_NAME||pc.BLACK_IDNUM||'/'||pc.BLACK_YEAR ELSE '-' END CASE_BLACE_NO, 
--
pbsb.LIMIT_AMOUNT,
--
plt.TITLE_NAME || pbb.FULLNAME  BAILMAN_NAME,
THAISDATE(pbsb.BANKGUARANTEE_DATE) BANKGUARANTEE_DATE ,
pbsb.BANKGUARANTEE_NO,
plb.BANK_NAME 
--
FROM PC_BAIL_SEC_BANKGUARANTEE pbsb
LEFT JOIN PC_BAIL_SEC pbs ON pbs.SEC_ID = pbsb.SEC_ID 
LEFT JOIN PC_BAIL pb ON pb.BAIL_ID = pbs.BAIL_ID 
LEFT JOIN PC_BAIL_BAILSMAN pbb ON pbb.BAILSMAN_ID = pbs.BAILSMAN_ID 
LEFT JOIN PC_LOOKUP_TITLE plt ON(plt.TITLE_ID=pbb.NAME_TITLE_ID AND plt.REMOVED_BY = 0)
--	  
LEFT JOIN PC_CASE pc ON pc.CASE_ID = pb.CASE_ID AND pc.REMOVED_BY = 0
LEFT JOIN PC_LOOKUP_TITLE_CASE pltc_b ON pltc_b.TITLE_CASE_ID = pc.BLACK_TITLE_ID AND pltc_b.REMOVED_BY = 0
--
LEFT JOIN PC_LOOKUP_BANK plb ON plb.BANK_ID = pbsb.BANK_ID AND plb.REMOVED_BY = 0
--
LEFT JOIN (SELECT plc.COURT_NAME_TH
			FROM PC_PARAM pp
			JOIN PC_LOOKUP_COURT plc ON plc.COURT_ID = pp.PARAM_VALUE AND plc.REMOVED_BY = 0
			WHERE pp.PARAM_NAME = 'COURT_ID' AND pp.REMOVED_BY = 0) cou ON 1 = 1
--
WHERE pbsb.REMOVED_BY = 0 
AND TO_DATE(TO_CHAR(pb.BAIL_DATE, 'MM-YYYY'),'MM-YYYY') = TO_DATE($P{SELECT_MONTH}, 'MM-YYYY')]]>
	</queryString>
