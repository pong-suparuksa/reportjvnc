		</field>
		<variable name="A_DAY" class="java.lang.Integer">
			<initialValueExpression><![CDATA[1]]></initialValueExpression>
		</variable>
	</subDataset>
	<subDataset name="DSET_APPOINT_CONCASE" uuid="43ed65da-76de-4508-90c5-a46318ec045f">
		<property name="com.jaspersoft.studio.data.defaultdataadapter" value="JVN210"/>
		<property name="com.jaspersoft.studio.data.sql.tables" value=""/>
		<property name="com.jaspersoft.studio.data.sql.SQLQueryDesigner.sash.w1" value="0"/>
		<property name="com.jaspersoft.studio.data.sql.SQLQueryDesigner.sash.w2" value="1000"/>
		<parameter name="START_DATE" class="java.lang.String">
			<defaultValueExpression><![CDATA["01/05/2021"]]></defaultValueExpression>
		</parameter>
		<parameter name="END_DATE" class="java.lang.String">
			<defaultValueExpression><![CDATA["31/05/2021"]]></defaultValueExpression>
		</parameter>
		<queryString>
			<![CDATA[SELECT A.CASE_BLACK_NO
       ,A.CASE_RED_NO
       ,A.PROS_DESC
       ,A.ACCU_DESC
       ,A.ALLE_DESC
       ,LISTAGG(THAILDATE(A.START_DATE,1,0,0), CHR(13)) WITHIN GROUP(ORDER BY A.START_DATE)   APPOINT_DATE
       ,LISTAGG(A.APPOINT_REASON, CHR(13)) WITHIN GROUP(ORDER BY A.START_DATE) APPOINT_REASON
       ,SUM(APPOINT_COUNT) APPOINT_COUNT
       ,A.JUDGE_NAME
       --check
       ,A.CASE_ID
FROM
(
SELECT pc.CASE_ID
       ,pltc_b.TITLE_CASE_NAME||pc.BLACK_IDNUM||'/'||pc.BLACK_YEAR CASE_BLACK_NO
       ,CASE WHEN pj.RED_IDNUM IS NULL THEN ' ' ELSE pltc_r.TITLE_CASE_NAME||pj.RED_IDNUM||'/'||pj.RED_YEAR END CASE_RED_NO
       ,NVL(pc.PROS_DESC, pc.COMPLAINANT_DESC) PROS_DESC
       ,pc.ACCU_DESC
       ,pc.ALLE_DESC
	   ,ppac.START_DATE
	   ,plal.APPOINT_LIST_NAME APPOINT_REASON
       ,SUM(CASE WHEN ppac.CHOICE IN (1,2) THEN 0.5 
                 WHEN ppac.CHOICE = 3 THEN 1
                 ELSE 0 END) APPOINT_COUNT
       ,NVL(j.JUDGE_NAME,'-') JUDGE_NAME
FROM PC_PROCEED_APPOINT_CONTINUE ppac 
--appoint
JOIN PC_PROCEED_APPOINT ppa ON ppa.APPOINT_ID = ppac.APPOINT_ID 
                           AND ppa.APPOINT_TABLE_CODE = ppac.APPOINT_TABLE_CODE
                           AND ppa.REMOVED_BY = 0
--appoint_table
LEFT JOIN PC_PROCEED_APPOINT_TABLE ppat ON ppat.APPOINT_TABLE_CODE = ppa.APPOINT_TABLE_CODE AND ppat.REMOVED_BY = 0
--appoint reason
LEFT JOIN PC_LOOKUP_APPOINT_LIST plal ON plal.APPOINT_LIST_ID = ppac.REASON_APPOINT_ID AND plal.REMOVED_BY = 0
 --case
LEFT JOIN PC_CASE pc ON pc.CASE_ID = ppa.CASE_ID AND pc.REMOVED_BY = 0
LEFT JOIN PC_LOOKUP_TITLE_CASE pltc_b ON pltc_b.TITLE_CASE_ID = pc.BLACK_TITLE_ID AND pltc_b.REMOVED_BY = 0
--judgement
LEFT JOIN PC_JUDGEMENT pj ON pj.CASE_ID = ppa.CASE_ID AND pj.REMOVED_BY = 0
LEFT JOIN PC_LOOKUP_TITLE_CASE pltc_r ON pltc_r.TITLE_CASE_ID = pj.RED_TITLE_ID AND pltc_r.REMOVED_BY = 0
--judge
LEFT JOIN (SELECT pcj.CASE_ID
				  ,pcj.JUDGE_ID
				  ,plj.JUDGE_NAME 
			      ,CASE WHEN pcj.TEMPORARY_JUDGE_FLAG = 3 THEN 'continuous'
			            WHEN pcj.TEMPORARY_JUDGE_FLAG = 4 THEN 'special' END APPOINT_TABLE_TYPE
		   FROM PC_CASE_JUDGE pcj 
		   JOIN PC_LOOKUP_JUDGE plj ON plj.JUDGE_ID = pcj.JUDGE_ID AND plj.REMOVED_BY = 0
		   JOIN PC_LOOKUP_JUDGE_TYPE pljt ON pljt.JUDGE_TYPE_ID = pcj.JUDGE_TYPE_ID AND pljt.REMOVED_BY = 0
		   WHERE pcj.REMOVED_BY = 0
		   AND pljt.JUDGE_TYPE_CODE = '01') j ON j.CASE_ID = ppa.CASE_ID AND j.APPOINT_TABLE_TYPE = ppat.APPOINT_TABLE_TYPE 
--
WHERE ppac.REMOVED_BY = 0
AND ppa.APPOINT_TABLE_CODE = '003'
AND ppac.START_DATE BETWEEN TO_DATE($P{START_DATE}||'T00:00:00', 'DD/MM/YYYY"T"HH24:MI:SS') 
                        AND TO_DATE($P{END_DATE}||'T23:59:59', 'DD/MM/YYYY"T"HH24:MI:SS')
AND NOT EXISTS(SELECT 1 FROM PC_PROCEED_APPOINT_CONTINUE WHERE REF_APPOINT_CON_ID  = ppac.APPOINT_CON_ID)
AND NOT EXISTS(SELECT 1 FROM PC_PROCEED_APPOINT_CONTINUE_CANCEL ppacc WHERE ppacc.APPOINT_CON_ID = ppac.APPOINT_CON_ID)
AND pltc_b.TITLE_CASE_NAME NOT IN ('มต','ตจ','มต.','ตจ.')	
GROUP BY pc.CASE_ID
 		 ,pltc_b.TITLE_CASE_NAME, pc.BLACK_IDNUM, pc.BLACK_YEAR
         ,pltc_r.TITLE_CASE_NAME, pj.RED_IDNUM, pj.RED_YEAR
         ,pc.PROS_DESC, pc.COMPLAINANT_DESC, pc.ACCU_DESC, pc.ALLE_DESC
         ,ppac.START_DATE, plal.APPOINT_LIST_NAME, j.JUDGE_NAME
) A
GROUP BY A.CASE_BLACK_NO, A.CASE_RED_NO, A.PROS_DESC, A.ACCU_DESC, A.ALLE_DESC, A.JUDGE_NAME, A.CASE_ID]]>
		</queryString>
