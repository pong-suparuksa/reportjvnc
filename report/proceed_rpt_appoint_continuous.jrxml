	<queryString language="SQL">
		<![CDATA[SELECT plc.COURT_NAME_TH,
             DECODE(EXTRACT(MONTH FROM TO_DATE($P{END_DATE},'DD/MM/YYYY')), 
       	       1, 'มกราคม', 2, 'กุมภาพันธ์', 3, 'มีนาคม', 4, 'เมษายน',
               5, 'พฤษภาคม', 6, 'มิถุนายน', 7,'กรกฎาคม', 8, 'สิงหาคม',                                                               
               9, 'กันยายน', 10, 'ตุลาคม', 11, 'พฤศจิกายน', 12, 'ธันวาคม', '-')                   SELECTED_MONTH, 
       EXTRACT(YEAR FROM TO_DATE($P{END_DATE}, 'DD/MM/YYYY'))+ 543   SELECTED_YEAR
FROM PC_PARAM pp
JOIN PC_LOOKUP_COURT plc ON plc.COURT_ID = pp.PARAM_VALUE AND plc.REMOVED_BY = 0
WHERE pp.PARAM_NAME = 'COURT_ID' AND pp.REMOVED_BY = 0]]>
	</queryString>
