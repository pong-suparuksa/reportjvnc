			<property name="com.jaspersoft.studio.field.label" value="WK_DAY"/>
		</field>
		<field name="WK_DAY_SHORT" class="java.lang.String">
			<property name="com.jaspersoft.studio.field.label" value="WK_DAY_SHORT"/>
		</field>
		<field name="APPOINT_DESC" class="java.lang.String">
			<property name="com.jaspersoft.studio.field.label" value="APPOINT_DESC"/>
		</field>
		<field name="WK_DAY_SHOW" class="java.lang.Integer">
			<property name="com.jaspersoft.studio.field.label" value="WK_DAY_SHOW"/>
		</field>
		<field name="A_ROW" class="java.lang.Integer">
