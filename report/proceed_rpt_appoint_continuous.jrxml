	<subDataset name="DSET_APPOINT_CON" uuid="9d43052b-6af2-4c09-b434-f7463d94285b">
		<property name="com.jaspersoft.studio.data.sql.tables" value=""/>
		<property name="com.jaspersoft.studio.data.defaultdataadapter" value="JVN210"/>
		<property name="com.jaspersoft.studio.data.sql.SQLQueryDesigner.sash.w1" value="0"/>
		<property name="com.jaspersoft.studio.data.sql.SQLQueryDesigner.sash.w2" value="1000"/>
		<parameter name="FROM_DATE" class="java.lang.String">
			<defaultValueExpression><![CDATA["01/09/2021"]]></defaultValueExpression>
		</parameter>
		<parameter name="TO_DATE" class="java.lang.String">
			<defaultValueExpression><![CDATA["30/09/2021"]]></defaultValueExpression>
		</parameter>
		<queryString language="SQL">
			<![CDATA[SELECT  A.A_DATE COL_DAY 
        ,A.WK_DAY
        ,A.WK_DAY_SHORT
        ,B.APPOINT_DESC
        ,C.WK_DAY_SHOW
        ,ROW_NUMBER() OVER (PARTITION BY A.A_DATE ORDER BY  A.A_DATE) A_ROW
--crosstab column
FROM (SELECT  EXTRACT(DAY FROM A.DT) A_DATE  
		       ,DECODE(TO_CHAR(A.DT,'D'),'1','อาทิตย์','2','จันทร์','3','อังคาร','4','พุธ','5','พฤหัสฯ','6','ศุกร์','7','เสาร์') WK_DAY 
		       ,A.DT
		       ,TO_CHAR(A.DT,'DY','NLS_DATE_LANGUAGE = ENGLISH') WK_DAY_SHORT
		FROM (SELECT TO_DATE($P{FROM_DATE},'DD/MM/YYYY') + ROWNUM -1 DT
			  FROM ALL_OBJECTS
			  WHERE ROWNUM <= TO_DATE($P{TO_DATE},'DD/MM/YYYY')-TO_DATE($P{FROM_DATE},'DD/MM/YYYY')+1) A ) A
--crosstab measure
LEFT JOIN (SELECT A.APPOINT_DATE
                  ,A.APPOINT_TABLE_CODE
			      ,A.APPOINT_DESC
			FROM(SELECT DISTINCT
				        ppac.START_DATE APPOINT_DATE
				        ,ppac.APPOINT_TABLE_CODE
					    ,pltc.TITLE_CASE_NAME||pc.BLACK_IDNUM||'/'||pc.BLACK_YEAR APPOINT_DESC
				 FROM PC_PROCEED_APPOINT_CONTINUE ppac 
				 --appoint
				 JOIN PC_PROCEED_APPOINT ppa ON ppa.APPOINT_ID = ppac.APPOINT_ID 
				                            AND ppa.APPOINT_TABLE_CODE = ppac.APPOINT_TABLE_CODE
				                            AND ppa.REMOVED_BY = 0
				 --case
				 LEFT JOIN PC_CASE pc ON pc.CASE_ID = ppa.CASE_ID AND pc.REMOVED_BY = 0
				 LEFT JOIN PC_LOOKUP_TITLE_CASE pltc ON pltc.TITLE_CASE_ID = pc.BLACK_TITLE_ID AND pltc.REMOVED_BY = 0
				 --
				 WHERE ppac.REMOVED_BY = 0
				 AND ppa.APPOINT_TABLE_CODE = '003'
				 AND NOT EXISTS(SELECT 1 FROM PC_PROCEED_APPOINT_CONTINUE WHERE REF_APPOINT_CON_ID  = ppac.APPOINT_CON_ID)
				 AND NOT EXISTS(SELECT 1 FROM PC_PROCEED_APPOINT_CONTINUE_CANCEL ppacc WHERE ppacc.APPOINT_CON_ID = ppac.APPOINT_CON_ID)
				 ) A
			GROUP BY A.APPOINT_DATE, A.APPOINT_TABLE_CODE, A.APPOINT_DESC) B ON B.APPOINT_DATE = A.DT
--day in appoint table			
LEFT JOIN (SELECT APPOINT_TABLE_CODE, WK_DAY_SHORT, WK_DAY_SHOW 
		   FROM PC_PROCEED_APPOINT_TABLE ppat 
		   UNPIVOT(WK_DAY_SHOW FOR WK_DAY_SHORT IN (SUN, MON, TUE, WED, THU, FRI, SAT))
		   WHERE APPOINT_TABLE_CODE = '003') C ON C.WK_DAY_SHORT = A.WK_DAY_SHORT
ORDER BY A.A_DATE]]>
		</queryString>
		<field name="COL_DAY" class="java.lang.Integer">
			<property name="com.jaspersoft.studio.field.label" value="COL_DAY"/>
		</field>
