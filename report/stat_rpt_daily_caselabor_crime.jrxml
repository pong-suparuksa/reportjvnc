			WHERE pc.REMOVED_BY = 0
			AND pj.CASE_ID IS NULL 
			GROUP BY pph.HOLD_REASON_ID) h3 ON h3.HOLD_REASON_ID = pphr.HOLD_REASON_ID 
ORDER BY pphr.HOLD_REASON_ID]]>
