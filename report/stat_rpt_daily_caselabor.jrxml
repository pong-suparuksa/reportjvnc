	<subDataset name="DS3_JUDGE" uuid="4bee3a4c-489a-4a8f-b85b-680d83badfa9">
		<property name="com.jaspersoft.studio.data.defaultdataadapter" value="RemoteCojStage2"/>
		<property name="com.jaspersoft.studio.data.sql.tables" value=""/>
		<property name="com.jaspersoft.studio.property.dataset.dialog.DatasetDialog.sash.w1" value="608"/>
		<property name="com.jaspersoft.studio.property.dataset.dialog.DatasetDialog.sash.w2" value="382"/>
		<property name="com.jaspersoft.studio.data.sql.SQLQueryDesigner.sash.w1" value="0"/>
		<property name="com.jaspersoft.studio.data.sql.SQLQueryDesigner.sash.w2" value="1000"/>
		<parameter name="FROM_DATE" class="java.lang.String">
			<defaultValueExpression><![CDATA["01/07/2020"]]></defaultValueExpression>
		</parameter>
		<parameter name="TO_DATE" class="java.lang.String">
			<defaultValueExpression><![CDATA["25/08/2020"]]></defaultValueExpression>
		</parameter>
		<queryString>
			<![CDATA[SELECT plj.JUDGE_ID,
       plj.JUDGE_NAME,
       SUM(NVL(pc1.CNT,0)) CNT_OWNER,
       SUM(NVL(pc2.CNT,0)) CNT_JUDGE_VICTORY,
       SUM(NVL(pc3.CNT,0)) CNT_JUDGE_MEDIATE,
       SUM(NVL(pc4.CNT,0)) CNT_JUDGE_WITHDRAW,
       SUM(NVL(pc5.CNT,0)) CNT_DISCHARGE,
       SUM(NVL(pc6.CNT,0)) CNT_OTHER_REASON,
       SUM(NVL(pc7.CNT,0)) CNT_TRANSFER_CASE,
       plj.SUPER_ID 
FROM PC_LOOKUP_JUDGE plj
--เจ้าของสำนวน
LEFT JOIN (SELECT pcj.JUDGE_ID, COUNT(pc.CASE_ID) CNT
			FROM PC_CASE pc 
            JOIN PC_CASE_JUDGE pcj ON pcj.CASE_ID = pc.CASE_ID AND pcj.REMOVED_BY = 0
            JOIN PC_LOOKUP_JUDGE_TYPE pljt ON pljt.JUDGE_TYPE_ID = pcj.JUDGE_TYPE_ID AND pljt.REMOVED_BY = 0
			JOIN PC_LOOKUP_CASE_CATE plcc ON plcc.CASE_CATE_ID = pc.CASE_CATE_ID AND plcc.REMOVED_BY = 0
		    JOIN PC_LOOKUP_CASE_TYPE plct ON plct.CASE_TYPE_ID = plcc.CASE_TYPE_ID AND plct.REMOVED_BY = 0           
            WHERE pc.CASE_DATE BETWEEN TO_DATE($P{FROM_DATE}||'T00:00:00', 'DD-MM-YYYY"T"HH24:MI:SS') 
                                    AND TO_DATE($P{TO_DATE}||'T23:59:59', 'DD-MM-YYYY"T"HH24:MI:SS')
            AND pc.REMOVED_BY = 0
            AND pljt.JUDGE_TYPE_CODE = '01'
            AND plct.CASE_TYPE_CODE = '2'
            GROUP BY pcj.JUDGE_ID) pc1 ON pc1.JUDGE_ID = plj.JUDGE_ID 
--พิพากษาความเรียง
LEFT JOIN (SELECT pcj.JUDGE_ID, pj.VICTORY, COUNT(pc.CASE_ID) CNT
			FROM PC_CASE pc 
			JOIN PC_JUDGEMENT pj ON pj.CASE_ID = pc.CASE_ID AND pj.REMOVED_BY = 0
            JOIN PC_CASE_JUDGE pcj ON pcj.CASE_ID = pc.CASE_ID AND pcj.REMOVED_BY = 0
            JOIN PC_LOOKUP_JUDGE_TYPE pljt ON pljt.JUDGE_TYPE_ID = pcj.JUDGE_TYPE_ID AND pljt.REMOVED_BY = 0
            JOIN PC_LOOKUP_CASE_CATE plcc ON plcc.CASE_CATE_ID = pc.CASE_CATE_ID AND plcc.REMOVED_BY = 0
		    JOIN PC_LOOKUP_CASE_TYPE plct ON plct.CASE_TYPE_ID = plcc.CASE_TYPE_ID AND plct.REMOVED_BY = 0           
            WHERE pc.REMOVED_BY = 0
            AND pj.JUDGE_DATE BETWEEN TO_DATE($P{FROM_DATE}||'T00:00:00', 'DD-MM-YYYY"T"HH24:MI:SS') 
                                   AND TO_DATE($P{TO_DATE}||'T23:59:59', 'DD-MM-YYYY"T"HH24:MI:SS')
            AND NVL(pj.VICTORY,0) IN (1,2) 
            AND plct.CASE_TYPE_CODE = '2'            
            GROUP BY pcj.JUDGE_ID, pj.VICTORY) pc2 ON pc2.JUDGE_ID = plj.JUDGE_ID 
--ประนีประนอมพิพากษาตามยอม
LEFT JOIN (SELECT pcj.JUDGE_ID, pj.VICTORY, COUNT(pc.CASE_ID) CNT
			FROM PC_CASE pc 
			JOIN PC_JUDGEMENT pj ON pj.CASE_ID = pc.CASE_ID AND pj.REMOVED_BY = 0
			JOIN PC_MEDIATION pm ON pm.CASE_ID = pc.CASE_ID AND pm.REMOVED_BY = 0
            JOIN PC_CASE_JUDGE pcj ON pcj.CASE_ID = pc.CASE_ID AND pcj.REMOVED_BY = 0
            JOIN PC_LOOKUP_JUDGE_TYPE pljt ON pljt.JUDGE_TYPE_ID = pcj.JUDGE_TYPE_ID AND pljt.REMOVED_BY = 0
            JOIN PC_MEDIATION_RESULT pmr ON pmr.MEDIATION_RESULT_ID = pm.MEDIATION_RESULT_ID AND pmr.REMOVED_BY = 0
            JOIN PC_LOOKUP_CASE_CATE plcc ON plcc.CASE_CATE_ID = pc.CASE_CATE_ID AND plcc.REMOVED_BY = 0
		    JOIN PC_LOOKUP_CASE_TYPE plct ON plct.CASE_TYPE_ID = plcc.CASE_TYPE_ID AND plct.REMOVED_BY = 0                       
            WHERE pc.REMOVED_BY = 0
            AND pj.JUDGE_DATE BETWEEN TO_DATE($P{FROM_DATE}||'T00:00:00', 'DD-MM-YYYY"T"HH24:MI:SS') 
                                   AND TO_DATE($P{TO_DATE}||'T23:59:59', 'DD-MM-YYYY"T"HH24:MI:SS') 
            AND pmr.MEDIATION_RESULT_CODE = '01'
            AND plct.CASE_TYPE_CODE = '2'            
            GROUP BY pcj.JUDGE_ID, pj.VICTORY) pc3 ON pc3.JUDGE_ID = plj.JUDGE_ID
--ถอนฟ้องถอนคำร้อง
LEFT JOIN (SELECT pcj.JUDGE_ID, pj.VICTORY, COUNT(pc.CASE_ID) CNT
			FROM PC_CASE pc 
			JOIN PC_JUDGEMENT pj ON pj.CASE_ID = pc.CASE_ID AND pj.REMOVED_BY = 0
            JOIN PC_CASE_JUDGE pcj ON pcj.CASE_ID = pc.CASE_ID AND pcj.REMOVED_BY = 0
            JOIN PC_LOOKUP_JUDGE_TYPE pljt ON pljt.JUDGE_TYPE_ID = pcj.JUDGE_TYPE_ID AND pljt.REMOVED_BY = 0
            JOIN PC_LOOKUP_JUDGE_RESULT pljr ON pljr.JUDGE_RESULT_ID = pj.JUDGE_RESULT_ID AND pljr.REMOVED_BY = 0
            JOIN PC_LOOKUP_CASE_CATE plcc ON plcc.CASE_CATE_ID = pc.CASE_CATE_ID AND plcc.REMOVED_BY = 0
		    JOIN PC_LOOKUP_CASE_TYPE plct ON plct.CASE_TYPE_ID = plcc.CASE_TYPE_ID AND plct.REMOVED_BY = 0                                   
            WHERE pc.REMOVED_BY = 0
            AND pj.JUDGE_DATE BETWEEN TO_DATE($P{FROM_DATE}||'T00:00:00', 'DD-MM-YYYY"T"HH24:MI:SS') 
                                   AND TO_DATE($P{TO_DATE}||'T23:59:59', 'DD-MM-YYYY"T"HH24:MI:SS')  
            AND pljr.JUDGE_RESULT_CODE = '10'
            AND plct.CASE_TYPE_CODE = '2'            
            GROUP BY pcj.JUDGE_ID, pj.VICTORY) pc4 ON pc4.JUDGE_ID = plj.JUDGE_ID   
--จำหน่ายคดี
LEFT JOIN (SELECT pcj.JUDGE_ID, pj.VICTORY, COUNT(pc.CASE_ID) CNT
			FROM PC_CASE pc 
			JOIN PC_JUDGEMENT pj ON pj.CASE_ID = pc.CASE_ID AND pj.REMOVED_BY = 0
            JOIN PC_CASE_JUDGE pcj ON pcj.CASE_ID = pc.CASE_ID AND pcj.REMOVED_BY = 0
            JOIN PC_LOOKUP_JUDGE_TYPE pljt ON pljt.JUDGE_TYPE_ID = pcj.JUDGE_TYPE_ID AND pljt.REMOVED_BY = 0
            JOIN PC_LOOKUP_JUDGE_RESULT pljr ON pljr.JUDGE_RESULT_ID = pj.JUDGE_RESULT_ID AND pljr.REMOVED_BY = 0
            JOIN PC_LOOKUP_CASE_CATE plcc ON plcc.CASE_CATE_ID = pc.CASE_CATE_ID AND plcc.REMOVED_BY = 0
		    JOIN PC_LOOKUP_CASE_TYPE plct ON plct.CASE_TYPE_ID = plcc.CASE_TYPE_ID AND plct.REMOVED_BY = 0                                             
            WHERE pc.REMOVED_BY = 0
            AND pj.JUDGE_DATE BETWEEN TO_DATE($P{FROM_DATE}||'T00:00:00', 'DD-MM-YYYY"T"HH24:MI:SS') 
                                   AND TO_DATE($P{TO_DATE}||'T23:59:59', 'DD-MM-YYYY"T"HH24:MI:SS')  
            AND pljr.JUDGE_RESULT_CODE = '08'
            AND plct.CASE_TYPE_CODE = '2'            
            GROUP BY pcj.JUDGE_ID, pj.VICTORY) pc5 ON pc5.JUDGE_ID = plj.JUDGE_ID 
--โดยเหตุอื่น
LEFT JOIN (SELECT pcj.JUDGE_ID, pj.VICTORY, COUNT(pc.CASE_ID) CNT
			FROM PC_CASE pc 
			JOIN PC_JUDGEMENT pj ON pj.CASE_ID = pc.CASE_ID AND pj.REMOVED_BY = 0
            JOIN PC_CASE_JUDGE pcj ON pcj.CASE_ID = pc.CASE_ID AND pcj.REMOVED_BY = 0
            JOIN PC_LOOKUP_JUDGE_TYPE pljt ON pljt.JUDGE_TYPE_ID = pcj.JUDGE_TYPE_ID AND pljt.REMOVED_BY = 0
            JOIN PC_LOOKUP_JUDGE_RESULT pljr ON pljr.JUDGE_RESULT_ID = pj.JUDGE_RESULT_ID AND pljr.REMOVED_BY = 0
            JOIN PC_LOOKUP_CASE_CATE plcc ON plcc.CASE_CATE_ID = pc.CASE_CATE_ID AND plcc.REMOVED_BY = 0
		    JOIN PC_LOOKUP_CASE_TYPE plct ON plct.CASE_TYPE_ID = plcc.CASE_TYPE_ID AND plct.REMOVED_BY = 0                                                         
            WHERE pc.REMOVED_BY = 0
            AND pj.JUDGE_DATE BETWEEN TO_DATE($P{FROM_DATE}||'T00:00:00', 'DD-MM-YYYY"T"HH24:MI:SS') 
                                   AND TO_DATE($P{TO_DATE}||'T23:59:59', 'DD-MM-YYYY"T"HH24:MI:SS')  
            AND pljr.JUDGE_RESULT_CODE = '09'
            AND plct.CASE_TYPE_CODE = '2'            
            GROUP BY pcj.JUDGE_ID, pj.VICTORY) pc6 ON pc6.JUDGE_ID = plj.JUDGE_ID 
LEFT JOIN (SELECT pcj.JUDGE_ID, COUNT(pc.CASE_ID) CNT 
			FROM PC_CASE pc 
			JOIN PC_CASE_JUDGE pcj ON pcj.CASE_ID = pc.CASE_ID AND pcj.REMOVED_BY = 0
			JOIN PC_LOOKUP_JUDGE_TYPE pljt ON pljt.JUDGE_TYPE_ID = pcj.JUDGE_TYPE_ID AND pljt.REMOVED_BY = 0
            JOIN PC_LOOKUP_CASE_CATE plcc ON plcc.CASE_CATE_ID = pc.CASE_CATE_ID AND plcc.REMOVED_BY = 0
		    JOIN PC_LOOKUP_CASE_TYPE plct ON plct.CASE_TYPE_ID = plcc.CASE_TYPE_ID AND plct.REMOVED_BY = 0  			
			WHERE pc.REMOVED_BY = 0
			AND pc.CASE_DATE BETWEEN TO_DATE($P{FROM_DATE}||'T00:00:00', 'DD-MM-YYYY"T"HH24:MI:SS') 
                                 AND TO_DATE($P{TO_DATE}||'T23:59:59', 'DD-MM-YYYY"T"HH24:MI:SS')			
            AND pcj.TEMPORARY_JUDGE_FLAG = 2
			AND pljt.JUDGE_TYPE_CODE =  '01'
            AND plct.CASE_TYPE_CODE = '2'
			GROUP BY pcj.JUDGE_ID) pc7 ON pc7.JUDGE_ID = plj.JUDGE_ID
WHERE plj.REMOVED_BY = 0
AND plj.FLAG_JUDGE = 1
GROUP BY plj.JUDGE_ID, plj.JUDGE_NAME, plj.SUPER_ID 
ORDER BY plj.SUPER_ID ASC]]>
		</queryString>
