	<subDataset name="DSET_CIVIL_CASEBRANCH1" uuid="79aef8c1-f8b0-465a-926b-9ca36bf127fc">
		<property name="com.jaspersoft.studio.data.sql.tables" value=""/>
		<property name="com.jaspersoft.studio.data.sql.SQLQueryDesigner.sash.w1" value="0"/>
		<property name="com.jaspersoft.studio.data.sql.SQLQueryDesigner.sash.w2" value="1000"/>
		<property name="com.jaspersoft.studio.data.defaultdataadapter" value="RemoteCoj160"/>
		<parameter name="SEL_MONTH" class="java.lang.String"/>
		<queryString language="SQL">
			<![CDATA[SELECT A.CATE_ORDER, A.CASE_CATE_NAME, NVL(B.CNT,0) NEW_CASE
FROM (SELECT plcc.CASE_CATE_CODE, plcc.CASE_CATE_NAME,
             CASE WHEN plcc.CASE_CATE_CODE = '409' THEN 1
                  WHEN plcc.CASE_CATE_CODE = '410' THEN 2
                  WHEN plcc.CASE_CATE_CODE = '408' THEN 3
                  WHEN plcc.CASE_CATE_CODE = '412' THEN 4
                  WHEN plcc.CASE_CATE_CODE = '407' THEN 5
                  WHEN plcc.CASE_CATE_CODE = '404' THEN 6
                  WHEN plcc.CASE_CATE_CODE = '405' THEN 8
                  WHEN plcc.CASE_CATE_CODE = '106' THEN 9
                  WHEN plcc.CASE_CATE_CODE = '419' THEN 10                  
                  ELSE 999 END AS CATE_ORDER
      FROM PC_LOOKUP_CASE_CATE plcc 
      JOIN  PC_LOOKUP_CASE_TYPE plct ON plct.CASE_TYPE_ID = plcc.CASE_TYPE_ID 
      WHERE plct.case_type_code = '4') A
--
LEFT JOIN (SELECT plcc.CASE_CATE_CODE, COUNT(pc.CASE_ID) CNT
           FROM PC_CASE pc 
           JOIN PC_LOOKUP_CASE_CATE plcc ON plcc.CASE_CATE_ID = pc.CASE_CATE_ID
           WHERE TO_CHAR(pc.CASE_DATE,'MMYYYY') = $P{SEL_MONTH} 
           AND pc.REMOVED_BY = 0      
           GROUP BY plcc.CASE_CATE_CODE) B ON B.CASE_CATE_CODE = A.CASE_CATE_CODE
--           
WHERE A.CATE_ORDER <> 999      
ORDER BY A.CATE_ORDER]]>
		</queryString>
		<field name="CATE_ORDER" class="java.math.BigDecimal">
			<property name="com.jaspersoft.studio.field.label" value="CATE_ORDER"/>
		</field>
		<field name="CASE_CATE_NAME" class="java.lang.String">
			<property name="com.jaspersoft.studio.field.label" value="CASE_CATE_NAME"/>
		</field>
		<field name="NEW_CASE" class="java.math.BigDecimal">
			<property name="com.jaspersoft.studio.field.label" value="NEW_CASE"/>
		</field>
	</subDataset>
	<subDataset name="DSET_CIVIL_CASEBRANCH2" uuid="e1fc4c3d-a6b7-4625-bf57-5bf87b803f59">
		<property name="com.jaspersoft.studio.data.sql.tables" value=""/>
		<property name="com.jaspersoft.studio.data.defaultdataadapter" value="RemoteCoj160"/>
		<property name="com.jaspersoft.studio.data.sql.SQLQueryDesigner.sash.w1" value="0"/>
		<property name="com.jaspersoft.studio.data.sql.SQLQueryDesigner.sash.w2" value="1000"/>
		<parameter name="SELMONTH" class="java.lang.String"/>
		<queryString language="SQL">
			<![CDATA[SELECT A.CATE_ORDER, A.CASE_CATE_NAME, NVL(B.CNT,0) NEW_CASE
FROM (SELECT plcc.CASE_CATE_CODE, plcc.CASE_CATE_NAME,
             CASE WHEN plcc.CASE_CATE_CODE = '405' THEN 11
                  WHEN plcc.CASE_CATE_CODE = '410' THEN 12
                  WHEN plcc.CASE_CATE_CODE = '411' THEN 13
                  WHEN plcc.CASE_CATE_CODE = '412' THEN 14
                  WHEN plcc.CASE_CATE_CODE = '417' THEN 15
                  WHEN plcc.CASE_CATE_CODE = '424' THEN 16
                  WHEN plcc.CASE_CATE_CODE = '404' THEN 17
                  WHEN plcc.CASE_CATE_CODE = '415' THEN 18
                  WHEN plcc.CASE_CATE_CODE = '418' THEN 19                  
                  ELSE 999 END AS CATE_ORDER
      FROM PC_LOOKUP_CASE_CATE plcc 
      JOIN  PC_LOOKUP_CASE_TYPE plct ON plct.CASE_TYPE_ID = plcc.CASE_TYPE_ID 
      WHERE plct.case_type_code = '4') A
--
LEFT JOIN (SELECT plcc.CASE_CATE_CODE, COUNT(pc.CASE_ID) CNT
           FROM PC_CASE pc 
           JOIN PC_LOOKUP_CASE_CATE plcc ON plcc.CASE_CATE_ID = pc.CASE_CATE_ID
           WHERE TO_CHAR(pc.CASE_DATE,'MMYYYY') = $P{SELMONTH} 
           AND pc.REMOVED_BY = 0      
           GROUP BY plcc.CASE_CATE_CODE) B ON B.CASE_CATE_CODE = A.CASE_CATE_CODE
--           
WHERE A.CATE_ORDER <> 999      
ORDER BY A.CATE_ORDER]]>
		</queryString>
		<field name="CATE_ORDER" class="java.math.BigDecimal">
			<property name="com.jaspersoft.studio.field.label" value="CATE_ORDER"/>
		</field>
		<field name="CASE_CATE_NAME" class="java.lang.String">
			<property name="com.jaspersoft.studio.field.label" value="CASE_CATE_NAME"/>
		</field>
		<field name="NEW_CASE" class="java.math.BigDecimal">
			<property name="com.jaspersoft.studio.field.label" value="NEW_CASE"/>
		</field>
		<variable name="SUM_NEWCASE" class="java.lang.Long" calculation="Sum">
			<variableExpression><![CDATA[$F{NEW_CASE}]]></variableExpression>
		</variable>
	</subDataset>
	<parameter name="SELECTED_MONTH" class="java.lang.String">
		<defaultValueExpression><![CDATA["032020"]]></defaultValueExpression>
	</parameter>
	<queryString language="SQL">
		<![CDATA[SELECT plc.COURT_NAME_TH,
       DECODE(SUBSTR($P{SELECTED_MONTH} ,1,2), 
       	                             '01', 'มกราคม', 
                                     '02', 'กุมภาพันธ์', 
                                     '03', 'มีนาคม', 
                                     '04', 'เมษายน',
                                     '05', 'พฤษภาคม',
                                     '06', 'มิถุนายน',  
                                     '07','กรกฎาคม',
                                     '08', 'สิงหาคม',                                                               
                                     '09', 'กันยายน', 
                                     '10', 'ตุลาคม', 
                                     '11', 'พฤศจิกายน',
                                     '12', 'ธันวาคม', '-')                SELECTED_MONTH, 
       SUBSTR($P{SELECTED_MONTH} ,-4,4)+ 543    SELECTED_YEAR,
       T.CNT  TOTAL_CIVIL_INLAW,
       TT.CNT TOTAL_ALLCASE
FROM PC_PARAM pp
JOIN PC_LOOKUP_COURT plc ON plc.COURT_ID = pp.PARAM_VALUE AND plc.REMOVED_BY = 0
CROSS JOIN (SELECT COUNT(pc.CASE_ID) CNT 
             FROM(SELECT CASE WHEN pla.ALLEGATION_CODE = '21005' THEN 1
		                       WHEN pla.ALLEGATION_CODE = '21004' THEN 2 
		                       WHEN pla.ALLEGATION_CODE = '21003' THEN 3
		      				   WHEN pla.ALLEGATION_CODE = '21006' THEN 4
		      				   WHEN pla.ALLEGATION_CODE = '21007' THEN 5
		      				   WHEN pla.ALLEGATION_CODE = '21008' THEN 6
		      			       WHEN pla.ALLEGATION_CODE = '21009' THEN 7
		                       WHEN pla.ALLEGATION_CODE = '21010' THEN 8
  						       WHEN pla.ALLEGATION_CODE = '21011' THEN 9
						       WHEN pla.ALLEGATION_CODE = '21012' THEN 10
						       WHEN pla.ALLEGATION_CODE = '21013' THEN 11
						       WHEN pla.ALLEGATION_CODE = '21014' THEN 12
						       WHEN pla.ALLEGATION_CODE = '21015' THEN 13
						       WHEN pla.ALLEGATION_CODE = '21018' THEN 14
						       WHEN pla.ALLEGATION_CODE = '21019' THEN 15
						       WHEN pla.ALLEGATION_CODE = '21020' THEN 16
						       WHEN pla.ALLEGATION_CODE = '21016' THEN 17
						       WHEN pla.ALLEGATION_CODE = '21021' THEN 18
						       WHEN pla.ALLEGATION_CODE = '21022' THEN 19
						       WHEN pla.ALLEGATION_CODE = '21025' THEN 20
						       WHEN pla.ALLEGATION_CODE = '90001' THEN 21
						       WHEN pla.ALLEGATION_CODE = '21026' THEN 22
						       WHEN pla.ALLEGATION_CODE = '21023' THEN 23
						       WHEN pla.ALLEGATION_CODE = '21010' THEN 23.1
				               WHEN pla.ALLEGATION_CODE = '21027' THEN 24
							   WHEN pla.ALLEGATION_CODE = '21028' THEN 25
							   WHEN pla.ALLEGATION_CODE = '21030' THEN 26
							   WHEN pla.ALLEGATION_CODE = '21031' THEN 28.02
							   WHEN pla.ALLEGATION_CODE = '21053' THEN 28.03
							   WHEN pla.ALLEGATION_CODE = '21054' THEN 28.04
							   WHEN pla.ALLEGATION_CODE = '21001' THEN 28.05
							   WHEN pla.ALLEGATION_CODE = '21068' THEN 28.06
							   WHEN pla.ALLEGATION_CODE = '21069' THEN 28.07
							   WHEN pla.ALLEGATION_CODE = '21032' THEN 28.09
							   WHEN pla.ALLEGATION_CODE = '21064' THEN 28.10
							   WHEN pla.ALLEGATION_CODE = '21063' THEN 28.12
							   WHEN pla.ALLEGATION_CODE = '21038' THEN 29.00
					           WHEN pla.ALLEGATION_CODE = '21067' THEN 29.01	
					           WHEN pla.ALLEGATION_CODE = '21039' THEN 30.00		      
						       ELSE 999 END CATE_ORDER,
				          pla.ALLEGATION_ID,
				          pla.ALLEGATION_NAME
					FROM PC_LOOKUP_ALLEGATION pla 
				    LEFT JOIN PC_LOOKUP_CASE_TYPE plct ON plct.CASE_TYPE_ID = pla.CASE_TYPE_ID
				    WHERE plct.CASE_TYPE_CODE = '2') A
			 LEFT JOIN PC_CASE_ALLE pca ON pca.ALLEGATION_ID = A.ALLEGATION_ID AND pca.REMOVED_BY = 0
			 LEFT JOIN PC_CASE pc ON pc.CASE_ID = pca.CASE_ID 
			                      AND TO_CHAR(pc.CASE_DATE,'MMYYYY') = $P{SELECTED_MONTH} 
			                      AND pc.REMOVED_BY = 0
			 WHERE A.CATE_ORDER <> 999) T
CROSS JOIN (SELECT COUNT(pc.CASE_ID) CNT 
             FROM PC_CASE pc 
             WHERE TO_CHAR(pc.CASE_DATE,'MMYYYY') =  $P{SELECTED_MONTH} 
             AND pc.REMOVED_BY = 0) TT
WHERE pp.PARAM_NAME = 'COURT_ID' AND pp.REMOVED_BY = 0]]>
	</queryString>
