	         WHEN plp.PENALTY_CODE = '04' THEN plp.PENALTY_NAME
	         WHEN plp.PENALTY_CODE = '06' THEN plp.PENALTY_NAME
	         WHEN plp.PENALTY_CODE = '10' THEN plp.PENALTY_NAME
	         WHEN plp.PENALTY_CODE = '18' THEN plp.PENALTY_NAME
	         WHEN plp.PENALTY_CODE = '14' THEN plp.PENALTY_NAME
	         WHEN plp.PENALTY_CODE = '13' THEN plp.PENALTY_NAME
	         --
