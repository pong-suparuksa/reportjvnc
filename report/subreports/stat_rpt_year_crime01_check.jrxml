	                  (A.LITIGANT_SEX = 2) THEN 1 ELSE 0 END) OTH_FEMALE
FROM (SELECT CASE 
			 WHEN plp.PENALTY_CODE = '17' THEN 1
			 WHEN plp.PENALTY_CODE = '04' THEN 2
			 WHEN plp.PENALTY_CODE = '06' THEN 3
	         WHEN plp.PENALTY_CODE = '10' THEN 4
	         WHEN plp.PENALTY_CODE = '18' THEN 5
	         WHEN plp.PENALTY_CODE = '14' THEN 6
	         WHEN plp.PENALTY_CODE = '13' THEN 7
	         --
	         WHEN plp.PENALTY_CODE = '08' THEN 8
	         WHEN plp.PENALTY_CODE = '02' THEN 9
	         WHEN plp.PENALTY_CODE = '09' THEN 10
	         --
	         WHEN plp.PENALTY_CODE = '05' THEN 13
	         WHEN plp.PENALTY_CODE = '07' THEN 14
	         ELSE 99 END PENALTY_ORDER,
	   CASE WHEN plp.PENALTY_CODE = '17' THEN 1
	         WHEN plp.PENALTY_CODE = '04' THEN 1
	         WHEN plp.PENALTY_CODE = '06' THEN 1
	         WHEN plp.PENALTY_CODE = '10' THEN 1
	         WHEN plp.PENALTY_CODE = '18' THEN 1
	         WHEN plp.PENALTY_CODE = '14' THEN 1
	         WHEN plp.PENALTY_CODE = '13' THEN 1
	         --
	         WHEN plp.PENALTY_CODE = '08' THEN 2
	         WHEN plp.PENALTY_CODE = '02' THEN 2
	         WHEN plp.PENALTY_CODE = '09' THEN 3
	         WHEN plp.PENALTY_CODE = '05' THEN 6
	         WHEN plp.PENALTY_CODE = '07' THEN 6
