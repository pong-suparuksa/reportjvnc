		<queryString language="SQL">
			<![CDATA[SELECT A.ALLEGATION_NAME,
       --
       CASE WHEN (B.LITIGANT_SEX = 1) AND (B.LITIGANT_AGE <= 13) THEN 1 ELSE 0 END MALE_L13,
       CASE WHEN (B.LITIGANT_SEX = 2) AND (B.LITIGANT_AGE <= 13) THEN 1 ELSE 0 END FEMALE_L13,
       --
       CASE WHEN (B.LITIGANT_SEX = 1) AND (B.LITIGANT_AGE >13 AND B.LITIGANT_AGE <= 15) THEN 1 ELSE 0 END MALE_13T15,
       CASE WHEN (B.LITIGANT_SEX = 2) AND (B.LITIGANT_AGE >13 AND B.LITIGANT_AGE <= 15) THEN 1 ELSE 0 END FEMALE_13T15,
       -- 
       CASE WHEN (B.LITIGANT_SEX = 1) AND (B.LITIGANT_AGE >15 AND B.LITIGANT_AGE <= 18) THEN 1 ELSE 0 END MALE_15T18,
       CASE WHEN (B.LITIGANT_SEX = 2) AND (B.LITIGANT_AGE >15 AND B.LITIGANT_AGE <= 18) THEN 1 ELSE 0 END FEMALE_15T18,
	   --
       CASE WHEN (B.LITIGANT_SEX = 1) AND (B.LITIGANT_AGE >18) THEN 1 ELSE 0 END MALE_M18,
       CASE WHEN (B.LITIGANT_SEX = 2) AND (B.LITIGANT_AGE >18) THEN 1 ELSE 0 END FEMALE_M18
       --
FROM(SELECT DISTINCT 
       pla.ALLEGATION_ID,
       pla.ALLEGATION_CODE, 
       pla.ALLEGATION_NAME,
       CASE WHEN pla.ALLEGATION_CODE = '11019' THEN 1 
            WHEN pla.ALLEGATION_CODE = '11020' THEN 2
            WHEN pla.ALLEGATION_CODE = '11021' THEN 3
            WHEN pla.ALLEGATION_CODE = '12033' THEN 4
       ELSE 999 END CASE_ORDER
	 FROM PC_LOOKUP_ALLEGATION pla) A	
--
LEFT JOIN(SELECT pcl.CASE_LIT_ID,
                  pca.ALLEGATION_ID,
      	          NVL(pcl.LITIGANT_SEX,0) LITIGANT_SEX,
	   		      CASE WHEN NVL(pcl.LITIGANT_AGE,0) > NVL(FLOOR(MONTHS_BETWEEN(CURRENT_DATE,pcl.LITIGANT_BIRTH_DATE)/12),0) 
	                   THEN pcl.LITIGANT_AGE  
	                   ELSE NVL(FLOOR(MONTHS_BETWEEN(CURRENT_DATE,pcl.LITIGANT_BIRTH_DATE)/12),0) 
	              END LITIGANT_AGE
	      FROM PC_CASE_LIT pcl
          JOIN PC_CASE_ALLE pca ON pca.CASE_ID = pcl.CASE_ID AND pca.REMOVED_BY = 0
          JOIN PC_CASE pc ON pc.CASE_ID = pcl.CASE_ID AND pc.REMOVED_BY = 0
          JOIN PC_JUDGEMENT pj ON pj.CASE_ID = pc.CASE_ID AND pj.REMOVED_BY = 0
          JOIN PC_LOOKUP_LIT_TYPE pllt ON pllt.LIT_TYPE_ID = pcl.LITIGANT_TYPE_ID AND pllt.REMOVED_BY = 0
          WHERE pllt.REMOVED_BY = 0
          AND pllt.LIT_TYPE_CODE = '14'
          AND EXTRACT(YEAR FROM pc.CASE_DATE) = $P{SELYEAR}) B ON B.ALLEGATION_ID = A.ALLEGATION_ID
--
WHERE A.CASE_ORDER <> 999
ORDER BY A.CASE_ORDER]]>
		</queryString>
