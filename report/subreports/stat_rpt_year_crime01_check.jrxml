	<subDataset name="DSET_CNTCASE" uuid="7ed3c68c-4653-4b00-9a88-90b1ffd4b1b5">
		<property name="com.jaspersoft.studio.data.sql.tables" value=""/>
		<property name="com.jaspersoft.studio.data.defaultdataadapter" value="RemoteCojTest"/>
		<property name="com.jaspersoft.studio.data.sql.SQLQueryDesigner.sash.w1" value="0"/>
		<property name="com.jaspersoft.studio.data.sql.SQLQueryDesigner.sash.w2" value="1000"/>
		<parameter name="SELYEAR" class="java.lang.Integer">
			<defaultValueExpression><![CDATA[2020]]></defaultValueExpression>
		</parameter>
		<queryString language="SQL">
			<![CDATA[SELECT plcc.CASE_CATE_NAME,
       NVL(pc1.CASE_REMAIN,0)  CASE_REMAIN, 
       NVL(pc2.CASE_NEW,0)     CASE_NEW,       
       NVL(pc3.JUDGEMENT_CLAIMANT,0)  JUDGEMENT_CLAIMANT,
       NVL(pc4.JUDGEMENT_DEFENDANT,0) JUDGEMENT_DEFENDANT, 
       NVL(pc5.JUDGEMENT_MEDIATE,0)   JUDGEMENT_MEDIATE,
       NVL(pc6.REQ_WITHDRAW,0)        REQ_WITHDRAW,
       NVL(pc7.CASE_DISCHARGE_TEMP,0) CASE_DISCHARGE_TEMP,
       NVL(pc8.CASE_DISCHARGE_OTH,0)  CASE_DISCHARGE_OTH,
       NVL(pc9.OTH_CAUSE,0)           OTH_CAUSE,
       NVL(pc10.CASE_NEW_REMAIN,0)    CASE_NEW_REMAIN,
       NVL(pc1.CASE_REMAIN,0)+NVL(pc10.CASE_NEW_REMAIN,0)  CASE_CARRYOUT
--
FROM PC_LOOKUP_CASE_CATE plcc
--pc1:คดีค้างมา
LEFT JOIN (SELECT pc.CASE_CATE_ID, COUNT(pc.CASE_ID) CASE_REMAIN 
            FROM PC_CASE pc 
            LEFT JOIN PC_JUDGEMENT pj ON pj.CASE_ID = pc.CASE_ID AND pj.REMOVED_BY = 0
            WHERE pc.REMOVED_BY = 0 
            AND EXTRACT(YEAR FROM ADD_MONTHS(pc.CASE_DATE,12))=  $P{SELYEAR} 
            AND pj.CASE_ID IS NULL
            GROUP BY pc.CASE_CATE_ID) pc1 ON pc1.CASE_CATE_ID = plcc.CASE_CATE_ID 
--pc2:คดีรับใหม่
LEFT JOIN (SELECT pc.CASE_CATE_ID, COUNT(pc.CASE_ID) CASE_NEW
            FROM PC_CASE pc 
            WHERE pc.REMOVED_BY = 0 
            AND EXTRACT(YEAR FROM pc.CASE_DATE)= $P{SELYEAR} 
            GROUP BY pc.CASE_CATE_ID) pc2 ON pc2.CASE_CATE_ID = plcc.CASE_CATE_ID                                                
--pc3:คดีเสร็จพิพากษาโจทก์ชนะ
LEFT JOIN (SELECT pc.CASE_CATE_ID, COUNT(pc.CASE_ID) JUDGEMENT_CLAIMANT 
			FROM PC_CASE pc
			JOIN PC_JUDGEMENT pj ON pj.CASE_ID = pc.CASE_ID 
								AND pj.REMOVED_BY = 0
								AND NVL(pj.VICTORY,0) = 1 
			WHERE pc.REMOVED_BY = 0			
			GROUP BY pc.CASE_CATE_ID) pc3 ON pc3.CASE_CATE_ID = plcc.CASE_CATE_ID
--pc4:คดีเสร็จพิพากษาจำเลยชนะ
LEFT JOIN (SELECT pc.CASE_CATE_ID, COUNT(pc.CASE_ID) JUDGEMENT_DEFENDANT 
			FROM PC_CASE pc
			JOIN PC_JUDGEMENT pj ON pj.CASE_ID = pc.CASE_ID 
								AND pj.REMOVED_BY = 0
								AND NVL(pj.VICTORY,0) = 2								
			WHERE pc.REMOVED_BY = 0
			GROUP BY pc.CASE_CATE_ID) pc4 ON pc4.CASE_CATE_ID = plcc.CASE_CATE_ID
--pc5:พิพากษาตามยอม(ประนีประนอม)
LEFT JOIN (SELECT pc.CASE_CATE_ID, COUNT(pc.CASE_ID) JUDGEMENT_MEDIATE 
			FROM PC_CASE pc
			JOIN PC_JUDGEMENT pj ON pj.CASE_ID = pc.CASE_ID 
								AND pj.REMOVED_BY = 0
            JOIN PC_LOOKUP_JUDGE_RESULT pljr ON pljr.JUDGE_RESULT_ID = pj.JUDGE_RESULT_ID
                                            AND pljr.REMOVED_BY = 0
                                            AND pljr.JUDGE_RESULT_CODE = '05'								 
			WHERE pc.REMOVED_BY = 0
			GROUP BY pc.CASE_CATE_ID) pc5 ON pc5.CASE_CATE_ID = plcc.CASE_CATE_ID
--pc6:ถอนฟ้อง ถอนคำร้องขอ
LEFT JOIN (SELECT pc.CASE_CATE_ID, COUNT(pc.CASE_ID) REQ_WITHDRAW 
			FROM PC_CASE pc
			JOIN PC_JUDGEMENT pj ON pj.CASE_ID = pc.CASE_ID 
								AND pj.REMOVED_BY = 0
            JOIN PC_LOOKUP_JUDGE_RESULT pljr ON pljr.JUDGE_RESULT_ID = pj.JUDGE_RESULT_ID
                                            AND pljr.REMOVED_BY = 0
                                            AND pljr.JUDGE_RESULT_CODE = '06'								 
			WHERE pc.REMOVED_BY = 0
			GROUP BY pc.CASE_CATE_ID) pc6 ON pc6.CASE_CATE_ID = plcc.CASE_CATE_ID			
--pc7:จำหน่ายคดีชั่วคราว
LEFT JOIN (SELECT pc.CASE_CATE_ID, COUNT(pc.CASE_ID) CASE_DISCHARGE_TEMP 
			FROM PC_CASE pc
			JOIN PC_JUDGEMENT pj ON pj.CASE_ID = pc.CASE_ID 
								AND pj.REMOVED_BY = 0
            JOIN PC_LOOKUP_JUDGE_RESULT pljr ON pljr.JUDGE_RESULT_ID = pj.JUDGE_RESULT_ID
                                            AND pljr.REMOVED_BY = 0
                                            AND pljr.JUDGE_RESULT_CODE = '07'								 
			WHERE pc.REMOVED_BY = 0
			GROUP BY pc.CASE_CATE_ID) pc7 ON pc7.CASE_CATE_ID = plcc.CASE_CATE_ID			
--pc8:จำหน่ายคดีโดยเหตุอื่น			
LEFT JOIN (SELECT pc.CASE_CATE_ID, COUNT(pc.CASE_ID) CASE_DISCHARGE_OTH 
			FROM PC_CASE pc
			JOIN PC_JUDGEMENT pj ON pj.CASE_ID = pc.CASE_ID 
								AND pj.REMOVED_BY = 0								
            JOIN PC_LOOKUP_JUDGE_RESULT pljr ON pljr.JUDGE_RESULT_ID = pj.JUDGE_RESULT_ID
                                            AND pljr.REMOVED_BY = 0
                                            AND pljr.JUDGE_RESULT_CODE = '08'								 
			WHERE pc.REMOVED_BY = 0
			GROUP BY pc.CASE_CATE_ID) pc8 ON pc8.CASE_CATE_ID = plcc.CASE_CATE_ID
--pc9:คดีเสร็จไปโดยเหตุอื่น			
LEFT JOIN (SELECT pc.CASE_CATE_ID, COUNT(pc.CASE_ID) OTH_CAUSE 
			FROM PC_CASE pc
			JOIN PC_JUDGEMENT pj ON pj.CASE_ID = pc.CASE_ID 
								AND pj.REMOVED_BY = 0
            JOIN PC_LOOKUP_JUDGE_RESULT pljr ON pljr.JUDGE_RESULT_ID = pj.JUDGE_RESULT_ID
                                            AND pljr.REMOVED_BY = 0
                                            AND pljr.JUDGE_RESULT_CODE = '09'								 
			WHERE pc.REMOVED_BY = 0
			GROUP BY pc.CASE_CATE_ID) pc9 ON pc9.CASE_CATE_ID = plcc.CASE_CATE_ID
--รับใหม่ปีนี้คงค้าง			
LEFT JOIN (SELECT pc.CASE_CATE_ID, COUNT(pc.CASE_ID) CASE_NEW_REMAIN 
            FROM PC_CASE pc 
            LEFT JOIN PC_JUDGEMENT pj ON pj.CASE_ID = pc.CASE_ID AND pj.REMOVED_BY = 0
            WHERE pc.REMOVED_BY = 0 
            AND EXTRACT(YEAR FROM pc.CASE_DATE)= $P{SELYEAR}             
            AND pj.CASE_ID IS NULL
            GROUP BY pc.CASE_CATE_ID) pc10 ON pc10.CASE_CATE_ID = plcc.CASE_CATE_ID 			
--
WHERE plcc.REMOVED_BY = 0
AND plcc.CASE_CATE_CODE = '101']]>
		</queryString>
