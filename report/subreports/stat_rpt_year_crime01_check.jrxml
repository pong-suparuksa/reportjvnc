		<property name="com.jaspersoft.studio.data.defaultdataadapter" value="RemoteCoj160"/>
		<property name="com.jaspersoft.studio.data.sql.SQLQueryDesigner.sash.w1" value="0"/>
		<property name="com.jaspersoft.studio.data.sql.SQLQueryDesigner.sash.w2" value="1000"/>
		<parameter name="SELYEAR" class="java.lang.Integer">
			<defaultValueExpression><![CDATA[2020]]></defaultValueExpression>
		</parameter>
		<queryString language="SQL">
			<![CDATA[SELECT 1 CASE_CODE,
       'ในคดีค้างมา' CASE_TYPE,
	   --สัญชาติไทย อายุเกิน 18 แต่ไ่เกิน 25 ปี
	   SUM(CASE WHEN (NVL(A.INTER_CODE,'0') = '10303') AND 
	                  (A.LITIGANT_AGE > 18 AND A.LITIGANT_AGE <= 25) AND
	                  (A.LITIGANT_SEX = 1) THEN 1 ELSE 0 END) THA_18T25_MALE,
	   SUM(CASE WHEN (NVL(A.INTER_CODE,'0') = '10303') AND 
	                  (A.LITIGANT_AGE > 18 AND A.LITIGANT_AGE <= 25) AND
	                  (A.LITIGANT_SEX = 2) THEN 1 ELSE 0 END) THA_18T25_FEMALE,
	   --สัญชาติไทย อายุเกิน 25 แต่ไ่เกิน 40 ปี
	   SUM(CASE WHEN (NVL(A.INTER_CODE,'0') = '10303') AND 
	                  (A.LITIGANT_AGE > 25 AND A.LITIGANT_AGE <= 40) AND
	                  (A.LITIGANT_SEX = 1) THEN 1 ELSE 0 END) THA_25T40_MALE,
	   SUM(CASE WHEN (NVL(A.INTER_CODE,'0') = '10303') AND 
	                  (A.LITIGANT_AGE > 25 AND A.LITIGANT_AGE <= 40) AND
	                  (A.LITIGANT_SEX = 2) THEN 1 ELSE 0 END) THA_25T40_FEMALE,    
	   --สัญชาติไทย อายุเกิน 40 แต่ไ่เกิน 60 ปี
	   SUM(CASE WHEN (NVL(A.INTER_CODE,'0') = '10303') AND 
	                  (A.LITIGANT_AGE > 40 AND A.LITIGANT_AGE <= 60) AND
	                  (A.LITIGANT_SEX = 1) THEN 1 ELSE 0 END) THA_40T60_MALE,
	   SUM(CASE WHEN (NVL(A.INTER_CODE,'0') = '10303') AND 
	                  (A.LITIGANT_AGE > 40 AND A.LITIGANT_AGE <= 60) AND
	                  (A.LITIGANT_SEX = 2) THEN 1 ELSE 0 END) THA_40T60_FEMALE, 
	   --สัญชาติไทย อายุเกิน 60 ปีขึ้นไป
	   SUM(CASE WHEN (NVL(A.INTER_CODE,'0') = '10303') AND 
	                  (A.LITIGANT_AGE > 60) AND
	                  (A.LITIGANT_SEX = 1) THEN 1 ELSE 0 END) THA_M60_MALE,
	   SUM(CASE WHEN (NVL(A.INTER_CODE,'0') = '10303') AND 
	                  (A.LITIGANT_AGE > 60) AND
