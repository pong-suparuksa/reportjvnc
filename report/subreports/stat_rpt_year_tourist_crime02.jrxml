		<queryString language="SQL">
			<![CDATA[SELECT A.CATE_ORDER,
       A.CATE_GROUP,
       A.ALLEGATION_NAME,
       COUNT(pc.CASE_ID) CNT 
FROM
(SELECT CASE WHEN pla.ALLEGATION_CODE = '11001' THEN 1.1
		      WHEN pla.ALLEGATION_CODE = '11002' THEN 1.2 
		      WHEN pla.ALLEGATION_CODE = '11003' THEN 1.3
		      WHEN pla.ALLEGATION_CODE = '11004' THEN 2.1
	          WHEN pla.ALLEGATION_CODE = '11006' THEN 2.2
		      WHEN pla.ALLEGATION_CODE = '11005' THEN 3.1
	          WHEN pla.ALLEGATION_CODE = '11007' THEN 3.2
              WHEN pla.ALLEGATION_CODE = '11008' THEN 4
              WHEN pla.ALLEGATION_CODE = '11009' THEN 5.1
              WHEN pla.ALLEGATION_CODE = '11010' THEN 5.2
              --
              WHEN pla.ALLEGATION_CODE = '11011' THEN 5.4
              WHEN pla.ALLEGATION_CODE = '11012' THEN 6.1
              WHEN pla.ALLEGATION_CODE = '11013' THEN 6.2
              --
              --
              --
              WHEN pla.ALLEGATION_CODE = '11015' THEN 7.1
              WHEN pla.ALLEGATION_CODE = '11016' THEN 7.2
              WHEN pla.ALLEGATION_CODE = '11017' THEN 7.3
              WHEN pla.ALLEGATION_CODE = '11018' THEN 8
              WHEN pla.ALLEGATION_CODE = '11019' THEN 9.1
              --
		      ELSE 999 END CATE_ORDER,
              CASE WHEN pla.ALLEGATION_CODE = '11001' THEN 1
		      WHEN pla.ALLEGATION_CODE = '11002' THEN 1
		      WHEN pla.ALLEGATION_CODE = '11003' THEN 1
		      WHEN pla.ALLEGATION_CODE = '11004' THEN 2
	          WHEN pla.ALLEGATION_CODE = '11006' THEN 2
		      WHEN pla.ALLEGATION_CODE = '11005' THEN 3
	          WHEN pla.ALLEGATION_CODE = '11007' THEN 3
              WHEN pla.ALLEGATION_CODE = '11008' THEN 3
              WHEN pla.ALLEGATION_CODE = '11009' THEN 5
              WHEN pla.ALLEGATION_CODE = '11010' THEN 5
              --
              WHEN pla.ALLEGATION_CODE = '11011' THEN 5
              WHEN pla.ALLEGATION_CODE = '11012' THEN 6
              WHEN pla.ALLEGATION_CODE = '11013' THEN 6
              --
              --
              --
              WHEN pla.ALLEGATION_CODE = '11015' THEN 7
              WHEN pla.ALLEGATION_CODE = '11016' THEN 7
              WHEN pla.ALLEGATION_CODE = '11017' THEN 7
              WHEN pla.ALLEGATION_CODE = '11018' THEN 7
              WHEN pla.ALLEGATION_CODE = '11019' THEN 9
              --
		      ELSE 999 END CATE_GROUP,
	   pla.ALLEGATION_ID,
       pla.ALLEGATION_NAME
FROM PC_LOOKUP_ALLEGATION pla 
LEFT JOIN PC_LOOKUP_CASE_TYPE plct ON plct.CASE_TYPE_ID = pla.CASE_TYPE_ID
WHERE plct.CASE_TYPE_CODE = '1') A
LEFT JOIN PC_CASE_ALLE pca ON pca.ALLEGATION_ID = A.ALLEGATION_ID AND pca.REMOVED_BY = 0
LEFT JOIN PC_CASE pc ON pc.CASE_ID = pca.CASE_ID 
                     AND EXTRACT(YEAR FROM pc.CASE_DATE) =  $P{SELYEAR} 
                     AND pc.REMOVED_BY = 0
WHERE A.CATE_ORDER <> 999
GROUP BY A.CATE_ORDER, A.CATE_GROUP, A.ALLEGATION_NAME
ORDER BY A.CATE_ORDER]]>
		</queryString>
