	<subDataset name="DSET_JUDGE" uuid="28fbee2f-16ba-49e6-9307-762037e6ed59">
		<property name="com.jaspersoft.studio.data.sql.tables" value=""/>
		<property name="com.jaspersoft.studio.data.defaultdataadapter" value="RemoteCoj160"/>
		<property name="com.jaspersoft.studio.data.sql.SQLQueryDesigner.sash.w1" value="0"/>
		<property name="com.jaspersoft.studio.data.sql.SQLQueryDesigner.sash.w2" value="1000"/>
		<parameter name="SELMONTH" class="java.lang.String">
			<defaultValueExpression><![CDATA["032020"]]></defaultValueExpression>
		</parameter>
		<queryString language="SQL">
			<![CDATA[SELECT THAISDATE(pc.CASE_DATE)  CASE_DATE_TEXT,
       pltc_b.TITLE_CASE_NAME || pc.BLACK_IDNUM || '/' || pc.BLACK_YEAR  CASE_BLACK_NUM,
       THAISDATE(pj.JUDGE_DATE) JUDGE_DATE_TEXT,
       pltc_r.TITLE_CASE_NAME || pj.RED_IDNUM  || '/' || pj.RED_YEAR     CASE_RED_NUM,
       pcalo.OFFENSE_NAME,
       --จำนวนคนที่ต้องโทษ
       CASE WHEN (NVL(pj.VICTORY,0) = 1) AND
                  (pli.INTER_CODE = '10303') AND
                  (pcl.LITIGANT_SEX = '1') THEN 1 ELSE 0 END PENALTY_THMALE,
       CASE WHEN (NVL(pj.VICTORY,0) = 1) AND
                  (pli.INTER_CODE = '10303') AND
                  (pcl.LITIGANT_SEX = '2') THEN 1 ELSE 0 END PENALTY_THFEMALE,
       CASE WHEN (NVL(pj.VICTORY,0) = 1) AND
                  (pli.INTER_CODE <> '10303') AND
                  (pcl.LITIGANT_SEX = '1') THEN 1 ELSE 0 END PENALTY_OTMALE,
       CASE WHEN (NVL(pj.VICTORY,0) = 1) AND
                  (pli.INTER_CODE <> '10303') AND
                  (pcl.LITIGANT_SEX = '2') THEN 1 ELSE 0 END PENALTY_OTFEMALE,      
       --จำนวนคนที่ศาลยกฟ้อง
       CASE WHEN (NVL(pj.VICTORY,0) = 2) AND
                  (pli.INTER_CODE = '10303') AND
                  (pcl.LITIGANT_SEX = '1') THEN 1 ELSE 0 END DISMISS_THMALE,
       CASE WHEN (NVL(pj.VICTORY,0) = 2) AND
                  (pli.INTER_CODE = '10303') AND
                  (pcl.LITIGANT_SEX = '2') THEN 1 ELSE 0 END DISMISS_THFEMALE,  
       CASE WHEN (NVL(pj.VICTORY,0) = 2) AND
                  (pli.INTER_CODE <> '10303') AND
                  (pcl.LITIGANT_SEX = '1') THEN 1 ELSE 0 END DISMISS_OTMALE,
       CASE WHEN (NVL(pj.VICTORY,0) = 2) AND
                  (pli.INTER_CODE <> '10303') AND
                  (pcl.LITIGANT_SEX = '2') THEN 1 ELSE 0 END DISMISS_OTFEMALE,
        --          
        NVL(pljr.JUDGE_RESULT_CODE,'0') JUDGE_RESULT_CODE, pljr.JUDGE_RESULT_NAME,
        --
        CASE WHEN(MONTHS_BETWEEN(pj.JUDGE_DATE,pc.CASE_DATE) >= 36)THEN 'เกิน 3 ปีขึ้นไป' 
             WHEN(MONTHS_BETWEEN(pj.JUDGE_DATE,pc.CASE_DATE) >= 24)THEN 'เกิน 2 ปีไม่เกิน 3 ปี'
             WHEN(MONTHS_BETWEEN(pj.JUDGE_DATE,pc.CASE_DATE) >= 12)THEN 'เกิน 1 ปีไม่เกิน 2 ปี'
             WHEN(MONTHS_BETWEEN(pj.JUDGE_DATE,pc.CASE_DATE) >= 6)THEN 'เกิน 6 เดือนไม่เกิน 1 ปี'
             WHEN(MONTHS_BETWEEN(pj.JUDGE_DATE,pc.CASE_DATE) >= 3)THEN 'เกิน 3 เดือนไม่เกิน 6 เดือน'
             WHEN(MONTHS_BETWEEN(pj.JUDGE_DATE,pc.CASE_DATE) >= 1)THEN 'เกิน 1 เดือนไม่เกิน 3 เดือน'
             ELSE 'ไม่เกิน 1 เดือน' END JUDGE_TIME_TEXT,
        --
        NVL(ap.APPEAL_SUBMIT_RESULT_NAME,'-') APPEAL_SUBMIT_RESULT,
        NVL(sp.APPEAL_SUBMIT_RESULT_NAME,'-') SUMPREME_SUBMIT_RESULT
FROM PC_CASE pc 
JOIN PC_CASE_LIT pcl ON pcl.CASE_ID = pc.CASE_ID AND pc.REMOVED_BY = 0
JOIN PC_JUDGEMENT pj ON pj.CASE_ID = pc.CASE_ID AND pj.REMOVED_BY = 0
LEFT JOIN PC_JUDGEMENT_ACCU_PENALTY pjap ON pjap.CASE_LIT_ID = pcl.CASE_LIT_ID AND pjap.REMOVED_BY = 0
LEFT JOIN (SELECT pca.CASE_ID, 
                   LISTAGG(plo.OFFENSE_CODE , ', ') WITHIN GROUP (ORDER BY plo.OFFENSE_NAME) OFFENSE_CODE,
                   LISTAGG(plo.OFFENSE_NAME , ', ') WITHIN GROUP (ORDER BY plo.OFFENSE_NAME) OFFENSE_NAME
            FROM PC_CASE_ALLE pca 
            JOIN PC_LOOKUP_OFFENSE plo ON plo.OFFENSE_ID = pca.ALLEGATION_ID AND plo.REMOVED_BY = 0
            WHERE pca.REMOVED_BY = 0
            GROUP BY pca.CASE_ID) pcalo ON pcalo.CASE_ID = pc.CASE_ID
--ศาลอุทธรณ์
LEFT JOIN (SELECT pas.CASE_LIT_ID, 
			       pasr.APPEAL_SUBMIT_RESULT_CODE,
			       pasr.APPEAL_SUBMIT_RESULT_NAME 
			FROM PC_APPEAL_SUBMIT pas
			JOIN PC_APPEAL_SUBMIT_RESULT pasr ON pasr.APPEAL_SUBMIT_RESULT_ID = pasr.APPEAL_SUBMIT_RESULT_ID AND pasr.REMOVED_BY = 0
			JOIN PC_APPEAL pa ON pa.APPEAL_ID = pas.APPEAL_ID AND pa.REMOVED_BY = 0
			JOIN PC_APPEAL_LEVEL pal ON pal.APPEAL_LEVEL_ID = pa.APPEAL_LEVEL_ID AND pal.REMOVED_BY = 0
			WHERE pal.APPEAL_LEVEL_CODE = '001') ap ON ap.CASE_LIT_ID = pcl.CASE_LIT_ID 
--ฎีกา
LEFT JOIN (SELECT pas.CASE_LIT_ID, 
			       pasr.APPEAL_SUBMIT_RESULT_CODE,
			       pasr.APPEAL_SUBMIT_RESULT_NAME 
			FROM PC_APPEAL_SUBMIT pas
			JOIN PC_APPEAL_SUBMIT_RESULT pasr ON pasr.APPEAL_SUBMIT_RESULT_ID = pasr.APPEAL_SUBMIT_RESULT_ID AND pasr.REMOVED_BY = 0
			JOIN PC_APPEAL pa ON pa.APPEAL_ID = pas.APPEAL_ID AND pa.REMOVED_BY = 0
			JOIN PC_APPEAL_LEVEL pal ON pal.APPEAL_LEVEL_ID = pa.APPEAL_LEVEL_ID AND pal.REMOVED_BY = 0
			WHERE pal.APPEAL_LEVEL_CODE = '002') sp ON sp.CASE_LIT_ID = pcl.CASE_LIT_ID 
--
LEFT JOIN PC_LOOKUP_TITLE_CASE pltc_b ON pltc_b.TITLE_CASE_ID = pc.BLACK_TITLE_ID AND pltc_b.REMOVED_BY = 0
LEFT JOIN PC_LOOKUP_TITLE_CASE pltc_r ON pltc_r.TITLE_CASE_ID = pj.RED_TITLE_ID AND pltc_r.REMOVED_BY = 0
LEFT JOIN PC_LOOKUP_LIT_TYPE pllt ON pllt.LIT_TYPE_ID = pcl.LITIGANT_TYPE_ID AND pllt.REMOVED_BY = 0
LEFT JOIN PC_LOOKUP_INTER pli ON pli.INTER_ID = pcl.INTER_ID AND pli.REMOVED_BY = 0
LEFT JOIN PC_LOOKUP_JUDGE_RESULT pljr ON pljr.JUDGE_RESULT_ID  = pj.JUDGE_RESULT_ID AND pljr.REMOVED_BY = 0
--
WHERE pc.REMOVED_BY = 0
AND TO_CHAR(pc.CASE_DATE, 'MMYYYY') =  $P{SELMONTH} 
AND pllt.LIT_TYPE_FLAG = '02']]>
		</queryString>
