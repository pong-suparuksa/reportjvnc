	<queryString language="SQL">
		<![CDATA[SELECT plc.COURT_NAME_TH,
       DECODE(SUBSTR($P{SELMONTH},1,2), 
       	                             '01', 'มกราคม', 
                                     '02', 'กุมภาพันธ์', 
                                     '03', 'มีนาคม', 
                                     '04', 'เมษายน',
                                     '05', 'พฤษภาคม',
                                     '06', 'มิถุนายน',  
                                     '07','กรกฎาคม',
                                     '08', 'สิงหาคม',                                                               
                                     '09', 'กันยายน', 
                                     '10', 'ตุลาคม', 
                                     '11', 'พฤศจิกายน',
                                     '12', 'ธันวาคม', '-')   SELECTED_MONTH, 
       SUBSTR($P{SELMONTH} ,-4,4)+ 543   SELECTED_YEAR
FROM PC_PARAM pp
JOIN PC_LOOKUP_COURT plc ON plc.COURT_CODE = pp.PARAM_VALUE AND plc.REMOVED_BY = 0
WHERE pp.PARAM_NAME = 'COURT_CODE' AND pp.REMOVED_BY = 0]]>
