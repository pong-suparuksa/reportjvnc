	<subDataset name="DSET_BAIL" uuid="a31a794a-e1e9-4cf7-b0c8-9a094bbfab74">
		<property name="com.jaspersoft.studio.data.sql.tables" value=""/>
		<property name="com.jaspersoft.studio.data.defaultdataadapter" value="RemoteCoj160"/>
		<property name="com.jaspersoft.studio.data.sql.SQLQueryDesigner.sash.w1" value="0"/>
		<property name="com.jaspersoft.studio.data.sql.SQLQueryDesigner.sash.w2" value="1000"/>
		<parameter name="SELMONTH" class="java.lang.String">
			<defaultValueExpression><![CDATA["032020"]]></defaultValueExpression>
		</parameter>
		<queryString language="SQL">
			<![CDATA[SELECT THAISDATE(pc.CASE_DATE) CASE_DATE_TEXT,
       pltc.TITLE_CASE_NAME || pc.BLACK_IDNUM || '/' || pc.BLACK_YEAR  CASE_BLACK_NUM,
       NVL(pcalo.OFFENSE_NAME,'-') OFFENSE_NAME,
       SUM(CASE WHEN (pli.INTER_CODE = '10303') AND (pcl.LITIGANT_SEX = '1') THEN 1 ELSE 0 END) DEFENDANT_THMALE,
       SUM(CASE WHEN (pli.INTER_CODE = '10303') AND (pcl.LITIGANT_SEX = '2') THEN 1 ELSE 0 END) DEFENDANT_THFEMALE,
       SUM(CASE WHEN (pli.INTER_CODE <> '10303') AND (pcl.LITIGANT_SEX = '1') THEN 1 ELSE 0 END) DEFENDANT_OTMALE,
       SUM(CASE WHEN (pli.INTER_CODE <> '10303') AND (pcl.LITIGANT_SEX = '2') THEN 1 ELSE 0 END) DEFENDANT_OTFEMALE,
       --
       SUM(CASE WHEN (NVL(pbd.COURT_ORDER,0)=1) AND
                      (pli.INTER_CODE = '10303') AND
                      (pcl.LITIGANT_SEX = '1') THEN 1 ELSE 0 END) BAILTH_PERMIT_MALE,
       SUM(CASE WHEN (NVL(pbd.COURT_ORDER,0)=1) AND
                      (pli.INTER_CODE = '10303') AND
                      (pcl.LITIGANT_SEX = '2') THEN 1 ELSE 0 END) BAILTH_PERMIT_FEMALE,
       SUM(CASE WHEN (NVL(pbd.COURT_ORDER,0)=1) AND
                      (pli.INTER_CODE <> '10303') AND
                      (pcl.LITIGANT_SEX = '1') THEN 1 ELSE 0 END) BAILOT_PERMIT_MALE,
       SUM(CASE WHEN (NVL(pbd.COURT_ORDER,0)=1) AND
                      (pli.INTER_CODE <> '10303') AND
                      (pcl.LITIGANT_SEX = '2') THEN 1 ELSE 0 END) BAILOT_PERMIT_FEMALE,
      --            
       SUM(CASE WHEN (NVL(pbd.COURT_ORDER,0)=2) AND
                      (pli.INTER_CODE = '10303') AND
                      (pcl.LITIGANT_SEX = '1') THEN 1 ELSE 0 END) BAILTH_CANCEL_MALE,
       SUM(CASE WHEN (NVL(pbd.COURT_ORDER,0)=2) AND
                      (pli.INTER_CODE = '10303') AND
                      (pcl.LITIGANT_SEX = '2') THEN 1 ELSE 0 END) BAILTH_CANCEL_FEMALE,
       SUM(CASE WHEN (NVL(pbd.COURT_ORDER,0)=2) AND
                      (pli.INTER_CODE <> '10303') AND
                      (pcl.LITIGANT_SEX = '1') THEN 1 ELSE 0 END) BAILOT_CANCEL_MALE,
       SUM(CASE WHEN (NVL(pbd.COURT_ORDER,0)=2) AND
                  (pli.INTER_CODE <> '10303') AND
                  (pcl.LITIGANT_SEX = '2') THEN 1 ELSE 0 END) BAILOT_CANCEL_FEMALE,
       --
       SUM(CASE WHEN (NVL(pbd.COURT_ORDER,0)=0) AND
                      (pli.INTER_CODE = '10303') AND
                      (pcl.LITIGANT_SEX = '1') THEN 1 ELSE 0 END) NOBAIL_THMALE,
       SUM(CASE WHEN (NVL(pbd.COURT_ORDER,0)=0) AND
                      (pli.INTER_CODE = '10303') AND
                      (pcl.LITIGANT_SEX = '2') THEN 1 ELSE 0 END) NOBAIL_THFEMALE,
       SUM(CASE WHEN (NVL(pbd.COURT_ORDER,0)=0) AND
                      (pli.INTER_CODE <> '10303') AND
                      (pcl.LITIGANT_SEX = '1') THEN 1 ELSE 0 END) NOBAIL_OTMALE,
       SUM(CASE WHEN (NVL(pbd.COURT_ORDER,0)=0) AND
                      (pli.INTER_CODE <> '10303') AND
                      (pcl.LITIGANT_SEX = '2') THEN 1 ELSE 0 END) NOBAIL_OTFEMALE,
       --
       SUM(CASE WHEN (NVL(pbbf.BAIL_ID,0)<>0) AND
                      (pli.INTER_CODE = '10303') AND
                      (pcl.LITIGANT_SEX = '1') THEN 1 ELSE 0 END) ESCAPE_THMALE, 
       SUM(CASE WHEN (NVL(pbbf.BAIL_ID,0)<>0) AND
                      (pli.INTER_CODE = '10303') AND
                      (pcl.LITIGANT_SEX = '2') THEN 1 ELSE 0 END) ESCAPE_THFEMALE,  
       SUM(CASE WHEN (NVL(pbbf.BAIL_ID,0)<>0) AND
                      (pli.INTER_CODE <> '10303') AND
                      (pcl.LITIGANT_SEX = '1') THEN 1 ELSE 0 END) ESCAPE_OTMALE,
       SUM(CASE WHEN (NVL(pbbf.BAIL_ID,0)<>0) AND
                      (pli.INTER_CODE <> '10303') AND
                      (pcl.LITIGANT_SEX = '2') THEN 1 ELSE 0 END) ESCAPE_OTFEMALE
	   --
FROM PC_CASE pc 
JOIN PC_CASE_LIT pcl ON pcl.CASE_ID = pc.CASE_ID AND pc.REMOVED_BY = 0
LEFT JOIN PC_BAIL_DETAIL pbd ON pbd.DEFENDANT_ID = pcl.CASE_LIT_ID AND pbd.REMOVED_BY = 0
LEFT JOIN PC_BAIL_BAILSMAN_FORFEIT pbbf ON pbbf.BAIL_ID = pbd.BAIL_ID AND pbbf.REMOVED_BY = 0
--
LEFT JOIN (SELECT pca.CASE_ID, LISTAGG(plo.OFFENSE_NAME , ', ') WITHIN GROUP (ORDER BY plo.OFFENSE_NAME) OFFENSE_NAME
                   FROM PC_CASE_ALLE pca 
                  JOIN PC_LOOKUP_OFFENSE plo ON plo.OFFENSE_ID = pca.ALLEGATION_ID AND plo.REMOVED_BY = 0
                 WHERE pca.REMOVED_BY = 0
                 GROUP BY pca.CASE_ID ) pcalo ON pcalo.CASE_ID = pc.CASE_ID
--            
LEFT JOIN PC_LOOKUP_TITLE_CASE pltc ON pltc.TITLE_CASE_ID = pc.BLACK_TITLE_ID AND pltc.REMOVED_BY = 0
LEFT JOIN PC_LOOKUP_LIT_TYPE pllt ON pllt.LIT_TYPE_ID = pcl.LITIGANT_TYPE_ID AND pllt.REMOVED_BY = 0
LEFT JOIN PC_LOOKUP_INTER pli ON pli.INTER_ID = pcl.INTER_ID AND pli.REMOVED_BY = 0
--
WHERE pc.REMOVED_BY = 0
AND TO_CHAR(pc.CASE_DATE, 'MMYYYY') =  $P{SELMONTH} 
AND pllt.LIT_TYPE_FLAG = '02'
--
GROUP BY THAISDATE(pc.CASE_DATE), 
                  pltc.TITLE_CASE_NAME || pc.BLACK_IDNUM || '/' || pc.BLACK_YEAR,
                  pcalo.OFFENSE_NAME
ORDER BY THAISDATE(pc.CASE_DATE), 
                  pltc.TITLE_CASE_NAME || pc.BLACK_IDNUM || '/' || pc.BLACK_YEAR]]>
		</queryString>
