	<subDataset name="DSET_REMAIN" uuid="d921f2e6-4464-49d2-baf8-f24e042a0a55">
		<property name="com.jaspersoft.studio.data.sql.tables" value=""/>
		<property name="com.jaspersoft.studio.data.defaultdataadapter" value="RemoteCoj160"/>
		<property name="com.jaspersoft.studio.data.sql.SQLQueryDesigner.sash.w1" value="0"/>
		<property name="com.jaspersoft.studio.data.sql.SQLQueryDesigner.sash.w2" value="1000"/>
		<parameter name="SELMONTH" class="java.lang.String">
			<defaultValueExpression><![CDATA["032020"]]></defaultValueExpression>
		</parameter>
		<parameter name="CURRENT_DATE" class="java.lang.String">
			<defaultValueExpression><![CDATA["31-03-2020"]]></defaultValueExpression>
		</parameter>
		<queryString language="SQL">
			<![CDATA[SELECT pltc.TITLE_CASE_NAME || pc.BLACK_IDNUM || '/' || pc.BLACK_YEAR  CASE_BLACK_NUM,
       THAISDATE(pc.CASE_DATE) CASE_DATE_TEXT,
       NVL(pcalo.OFFENSE_NAME,'-') OFFENSE_NAME,
       --
       MONTHS_BETWEEN(TO_TIMESTAMP($P{CURRENT_DATE},'DD-MM-YYYY'),pc.CASE_DATE) REMAIN,
       CASE WHEN(MONTHS_BETWEEN(TO_TIMESTAMP($P{CURRENT_DATE},'DD-MM-YYYY'),pc.CASE_DATE) >= 36)THEN 'เกิน 5 ปีขึ้นไป'
             WHEN(MONTHS_BETWEEN(TO_TIMESTAMP($P{CURRENT_DATE},'DD-MM-YYYY'),pc.CASE_DATE) >= 36)THEN 'เกิน 4 ปีไม่เกิน 5 ปี'
             WHEN(MONTHS_BETWEEN(TO_TIMESTAMP($P{CURRENT_DATE},'DD-MM-YYYY'),pc.CASE_DATE) >= 36)THEN 'เกิน 3 ปีไม่เกิน 4 ปี' 
             WHEN(MONTHS_BETWEEN(TO_TIMESTAMP($P{CURRENT_DATE},'DD-MM-YYYY'),pc.CASE_DATE) >= 24)THEN 'เกิน 2 ปีไม่เกิน 3 ปี'
             WHEN(MONTHS_BETWEEN(TO_TIMESTAMP($P{CURRENT_DATE},'DD-MM-YYYY'),pc.CASE_DATE) >= 12)THEN 'เกิน 1 ปีไม่เกิน 2 ปี'
             WHEN(MONTHS_BETWEEN(TO_TIMESTAMP($P{CURRENT_DATE},'DD-MM-YYYY'),pc.CASE_DATE) >= 6)THEN 'เกิน 6 เดือนไม่เกิน 1 ปี'
             WHEN(MONTHS_BETWEEN(TO_TIMESTAMP($P{CURRENT_DATE},'DD-MM-YYYY'),pc.CASE_DATE) >= 3)THEN 'เกิน 3 เดือนไม่เกิน 6 เดือน'
             WHEN(MONTHS_BETWEEN(TO_TIMESTAMP($P{CURRENT_DATE},'DD-MM-YYYY'),pc.CASE_DATE) >= 1)THEN 'เกิน 1 เดือนไม่เกิน 3 เดือน'
             ELSE 'ไม่เกิน 1 เดือน' END REMAIN_PERIOD
--
FROM PC_CASE pc 
JOIN PC_CASE_LIT pcl ON pcl.CASE_ID = pc.CASE_ID AND pc.REMOVED_BY = 0
LEFT JOIN PC_JUDGEMENT pj ON pj.CASE_ID = pc.CASE_ID 
--
LEFT JOIN (SELECT pca.CASE_ID, LISTAGG(plo.OFFENSE_NAME , ', ') WITHIN GROUP (ORDER BY plo.OFFENSE_NAME) OFFENSE_NAME
            FROM PC_CASE_ALLE pca 
            JOIN PC_LOOKUP_OFFENSE plo ON plo.OFFENSE_ID = pca.ALLEGATION_ID AND plo.REMOVED_BY = 0
            WHERE pca.REMOVED_BY = 0
            GROUP BY pca.CASE_ID ) pcalo ON pcalo.CASE_ID = pc.CASE_ID
--            
LEFT JOIN PC_LOOKUP_TITLE_CASE pltc ON pltc.TITLE_CASE_ID = pc.BLACK_TITLE_ID AND pltc.REMOVED_BY = 0
LEFT JOIN PC_LOOKUP_LIT_TYPE pllt ON pllt.LIT_TYPE_ID = pcl.LITIGANT_TYPE_ID AND pllt.REMOVED_BY = 0
--
WHERE pc.REMOVED_BY = 0
AND TO_CHAR(pc.CASE_DATE, 'MMYYYY') =  $P{SELMONTH} 
AND pj.CASE_ID IS NULL
AND pllt.LIT_TYPE_FLAG = '02']]>
		</queryString>
		<field name="CASE_BLACK_NUM" class="java.lang.String">
			<property name="com.jaspersoft.studio.field.label" value="CASE_BLACK_NUM"/>
		</field>
		<field name="CASE_DATE_TEXT" class="java.lang.String">
			<property name="com.jaspersoft.studio.field.label" value="CASE_DATE_TEXT"/>
		</field>
		<field name="OFFENSE_NAME" class="java.lang.String">
			<property name="com.jaspersoft.studio.field.label" value="OFFENSE_NAME"/>
		</field>
		<field name="REMAIN" class="java.math.BigDecimal">
			<property name="com.jaspersoft.studio.field.label" value="REMAIN"/>
		</field>
		<field name="REMAIN_PERIOD" class="java.lang.String">
			<property name="com.jaspersoft.studio.field.label" value="REMAIN_PERIOD"/>
		</field>
	</subDataset>
