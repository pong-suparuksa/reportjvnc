	<queryString>
		<![CDATA[SELECT
	ROW_NUMBER() OVER (
	ORDER BY pa.CHILD_AGE ) AS row_num ,
	pa.ADVICE_ID,
	pa.SP_CASE_ID ,
	pa.SP_CASE_YEAR ,
	pa.CASE_ID ,
	pa.CASE_YEAR ,
	pa.CHILD_PREFIX ,
	pa.CHILD_FIRSTNAME ,
	pa.CHILD_LASTNAME,
	pap.PARENT_PREFIX ,
	pap.PARENT_FIRSTNAME ,
	pap.PARENT_LASTNAME ,
	pap.PARENT_RELATION,
	NVL(plc.COURT_NAME_TH, '') COURT_NAME_TH
--
FROM PC_ADVICE pa
JOIN PC_ADVICE_PARENT pap ON pa.ADVICE_ID = pap.ADVICE_ID
--
LEFT JOIN PC_LOOKUP_COURT plc ON plc.COURT_ID = pa.COURT_ID AND plc.REMOVED_BY = 0	
--
WHERE
pa.CREATED_DATE BETWEEN TO_DATE($P{START_DATE}||'T00:00:00', 'DD-MM-YYYY"T"HH24:MI:SS')
                    AND TO_DATE($P{END_DATE}||'T23:59:59', 'DD-MM-YYYY"T"HH24:MI:SS')
	AND pa.REMOVED_BY = 0
 	AND pap.REMOVED_BY = 0]]>
	</queryString>
