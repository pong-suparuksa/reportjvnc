		<queryString language="SQL">
			<![CDATA[SELECT
	pae.EVALUATION_COUNT ,
	pae.EVALUATION_DATE ,
	pae.EVALUATION_TIME ,
	pae.TOPIC ,
	pae.CONSULTANT_NAME ,
	pae.ADVISOR_NAME ,
	pae.EVALUATION_RESULT ,
	pae.EVALUATION_REMARK ,
	FILE_NAME
	--NVL(paeaa.FILE_NAME, ',') AS FILE_NAME
FROM
	PC_ADVICE_EXT pa
JOIN PC_ADVICE_EXT_EVAL pae ON
	pae.ADVICE_EXT_ID = pa.ADVICE_EXT_ID
	AND pae.REMOVED_BY = 0
LEFT JOIN (
	SELECT
		EXT_EVAL_ID ,
		LISTAGG(FILE_NAME, ', ') WITHIN GROUP (
		ORDER BY FILE_NAME) FILE_NAME
	FROM
		PC_ADVICE_EXT_EVAL_ATTACHMENT
	GROUP BY
		EXT_EVAL_ID ) paeaa ON
	paeaa.EXT_EVAL_ID = pae.EXT_EVAL_ID
WHERE
	pa.CASE_ID = $P{CASE_ID}
	AND pa.CASE_YEAR = $P{CASE_YEAR}
ORDER BY pae.EVALUATION_DATE DESC]]>
		</queryString>
