			JOIN PC_APPEAL pa ON pa.CASE_ID = pc.CASE_iD AND pa.REMOVED_BY = 0
			JOIN PC_APPEAL_SUBMIT pas ON pas.APPEAL_ID = pa.APPEAL_ID AND pas.REMOVED_BY = 0
			JOIN PC_APPEAL_LEVEL pal ON pal.APPEAL_LEVEL_ID = pa.APPEAL_LEVEL_ID AND pal.REMOVED_BY = 0
