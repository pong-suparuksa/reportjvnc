AND pj.JUDGE_DATE BETWEEN TO_DATE($P{THISYEAR_FROMDATE}||'T00:00:00', 'DD-MM-YYYY"T"HH24:MI:SS') 
                      AND TO_DATE($P{THISYEAR_TODATE}||'T23:59:59', 'DD-MM-YYYY"T"HH24:MI:SS')

