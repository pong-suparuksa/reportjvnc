		<parameter name="FROM_DATE" class="java.lang.String"/>
		<parameter name="TO_DATE" class="java.lang.String"/>
		<queryString>
			<![CDATA[SELECT	plj.JUDGE_NAME,
    	plj.SUPER_ID,
       NVL(SUM(CASE WHEN plcc.CASE_CATE_CODE IN ('201','202','203','204','205','206','207','208','309', '209','310','311') AND pj.JUDGE_DATE IS NOT NULL AND pljt.JUDGE_TYPE_CODE = '01' THEN 1 ELSE 0 END),0)      CNT_OWNER,
       NVL(SUM(CASE WHEN plcc.CASE_CATE_CODE IN ('201','202','203','204','205','206','207','208','309', '209','310','311') AND pj.JUDGE_DATE IS NOT NULL AND NVL(pj.VICTORY,0) IN (1,2) THEN 1 ELSE 0 END),0)       CNT_JUDGE_VICTORY,
       NVL(SUM(CASE WHEN plcc.CASE_CATE_CODE IN ('201','202','203','204','205','206','207','208','309', '209','310','311') AND pj.JUDGE_DATE IS NOT NULL AND pmr.MEDIATION_RESULT_CODE = '01' THEN 1 ELSE 0 END),0) CNT_JUDGE_MEDIATE,
       NVL(SUM(CASE WHEN plcc.CASE_CATE_CODE IN ('201','202','203','204','205','206','207','208','309', '209','310','311') AND pj.JUDGE_DATE IS NOT NULL AND pljr.JUDGE_RESULT_CODE = '10' THEN 1 ELSE 0 END),0)    CNT_JUDGE_WITHDRAW,
       NVL(SUM(CASE WHEN plcc.CASE_CATE_CODE IN ('201','202','203','204','205','206','207','208','309', '209','310','311') AND pj.JUDGE_DATE IS NOT NULL AND pljr.JUDGE_RESULT_CODE = '07' THEN 1 ELSE 0 END),0)    CNT_TEMP_DISCHARGE,
       NVL(SUM(CASE WHEN plcc.CASE_CATE_CODE IN ('201','202','203','204','205','206','207','208','309', '209','310','311') AND pj.JUDGE_DATE IS NOT NULL AND pljr.JUDGE_RESULT_CODE = '08' THEN 1 ELSE 0 END),0)    CNT_OTHER_DISCHARGE,
       NVL(SUM(CASE WHEN plcc.CASE_CATE_CODE IN ('201','202','203','204','205','206','207','208','309', '209','310','311') AND pj.JUDGE_DATE IS NOT NULL AND pljr.JUDGE_RESULT_CODE = '09' THEN 1 ELSE 0 END),0)    CNT_OTHER_REASON
FROM PC_LOOKUP_JUDGE plj 
LEFT JOIN PC_CASE_JUDGE pcj ON pcj.JUDGE_ID = plj.JUDGE_ID AND pcj.REMOVED_BY = 0
LEFT JOIN PC_CASE pc ON pc.CASE_ID = pcj.CASE_ID AND pc.REMOVED_BY = 0
LEFT JOIN PC_JUDGEMENT pj ON pj.CASE_ID = pc.CASE_ID 
                          AND pj.REMOVED_BY = 0
                          AND pj.JUDGE_DATE BETWEEN TO_DATE($P{FROM_DATE}||'T00:00:00', 'DD-MM-YYYY"T"HH24:MI:SS') 
                                                AND TO_DATE($P{TO_DATE}||'T23:59:59', 'DD-MM-YYYY"T"HH24:MI:SS')
LEFT JOIN PC_LOOKUP_JUDGE_RESULT pljr ON pljr.JUDGE_RESULT_ID = pj.JUDGE_RESULT_ID AND pljr.REMOVED_BY = 0
--
LEFT JOIN PC_MEDIATION pm ON pm.CASE_ID = pc.CASE_ID AND pm.REMOVED_BY = 0
LEFT JOIN PC_MEDIATION_RESULT pmr ON pmr.MEDIATION_RESULT_ID = pm.MEDIATION_RESULT_ID AND pmr.REMOVED_BY = 0
--
LEFT JOIN PC_LOOKUP_JUDGE_TYPE pljt ON pljt.JUDGE_TYPE_ID = pcj.JUDGE_TYPE_ID AND pljt.REMOVED_BY = 0
LEFT JOIN PC_LOOKUP_CASE_CATE plcc ON plcc.CASE_CATE_ID = pc.CASE_CATE_ID AND plcc.REMOVED_BY = 0
LEFT JOIN PC_LOOKUP_CASE_TYPE plct ON plct.CASE_TYPE_ID = plcc.CASE_TYPE_ID AND plct.REMOVED_BY = 0 

WHERE plj.REMOVED_BY = 0
AND plj.FLAG_JUDGE = 1
GROUP BY plj.JUDGE_NAME, plj.SUPER_ID
ORDER BY plj.SUPER_ID]]>
		</queryString>
