SELECT 'คดีรับใหม่เพิ่มขึ้น' CASE_ITEM,
        CASE WHEN (A_CASE.CIV - A_JUDGE.CIV) < 0 THEN 0 
             ELSE (A_CASE.CIV - A_JUDGE.CIV) END CV_COUNT,
        CASE WHEN (A_CASE.CON - A_JUDGE.CON) < 0 THEN 0 
             ELSE (A_CASE.CON - A_JUDGE.CON) END CV_COUNT,
        CASE WHEN (A_CASE.ENV - A_JUDGE.ENV) < 0 THEN 0 
             ELSE (A_CASE.ENV - A_JUDGE.ENV) END CV_COUNT
FROM DUAL
CROSS JOIN (SELECT NVL(SUM(CASE WHEN plcc.CASE_CATE_CODE IN ('201','202','203','204','205','206','207','208','309') THEN 1 ELSE 0 END),0) CIV,
                    NVL(SUM(CASE WHEN plcc.CASE_CATE_CODE = '209' THEN 1 ELSE 0 END),0) CON,
                    NVL(SUM(CASE WHEN plcc.CASE_CATE_CODE IN ('310','311') THEN 1 ELSE 0 END),0) ENV
			 FROM PC_CASE pc
			 JOIN PC_LOOKUP_CASE_CATE plcc ON plcc.CASE_CATE_ID = pc.CASE_CATE_ID AND plcc.REMOVED_BY = 0
			 WHERE pc.REMOVED_BY = 0
			 AND TO_CHAR(pc.CASE_DATE,'DD-MM-YYYY') = $P{TODAY}) A_CASE
CROSS JOIN (SELECT NVL(SUM(CASE WHEN plcc.CASE_CATE_CODE IN ('201','202','203','204','205','206','207','208','309') THEN 1 ELSE 0 END),0) CIV,
                    NVL(SUM(CASE WHEN plcc.CASE_CATE_CODE = '209' THEN 1 ELSE 0 END),0) CON,
                    NVL(SUM(CASE WHEN plcc.CASE_CATE_CODE IN ('310','311') THEN 1 ELSE 0 END),0) ENV
			FROM PC_CASE pc
			JOIN PC_JUDGEMENT pj ON pj.CASE_ID = pc.CASE_ID AND pj.REMOVED_BY = 0
			JOIN PC_LOOKUP_CASE_CATE plcc ON plcc.CASE_CATE_ID = pc.CASE_CATE_ID AND plcc.REMOVED_BY = 0
			WHERE pc.REMOVED_BY = 0
			AND TO_CHAR(pj.JUDGE_DATE,'DD-MM-YYYY') = $P{TODAY}) A_JUDGE
UNION ALL
SELECT 'คดีรับใหม่ลดลง' CASE_ITEM,
        CASE WHEN (A_JUDGE.CIV - A_CASE.CIV) < 0 THEN 0 
             ELSE (A_JUDGE.CIV - A_CASE.CIV) END CV_COUNT,
        CASE WHEN (A_JUDGE.CON - A_CASE.CON) < 0 THEN 0 
             ELSE (A_JUDGE.CON - A_CASE.CON) END CS_COUNT,
        CASE WHEN (A_JUDGE.ENV - A_CASE.ENV) < 0 THEN 0 
             ELSE (A_JUDGE.ENV - A_CASE.ENV) END ES_COUNT
FROM DUAL
CROSS JOIN (SELECT NVL(SUM(CASE WHEN plcc.CASE_CATE_CODE IN ('201','202','203','204','205','206','207','208','309') THEN 1 ELSE 0 END),0) CIV,
                    NVL(SUM(CASE WHEN plcc.CASE_CATE_CODE = '209' THEN 1 ELSE 0 END),0) CON,
                    NVL(SUM(CASE WHEN plcc.CASE_CATE_CODE IN ('310','311') THEN 1 ELSE 0 END),0) ENV
			 FROM PC_CASE pc
			 JOIN PC_LOOKUP_CASE_CATE plcc ON plcc.CASE_CATE_ID = pc.CASE_CATE_ID AND plcc.REMOVED_BY = 0
			 WHERE pc.REMOVED_BY = 0
			 AND TO_CHAR(pc.CASE_DATE,'DD-MM-YYYY') = $P{TODAY}) A_CASE
CROSS JOIN (SELECT NVL(SUM(CASE WHEN plcc.CASE_CATE_CODE IN ('201','202','203','204','205','206','207','208','309') THEN 1 ELSE 0 END),0) CIV,
                    NVL(SUM(CASE WHEN plcc.CASE_CATE_CODE = '209' THEN 1 ELSE 0 END),0) CON,
                    NVL(SUM(CASE WHEN plcc.CASE_CATE_CODE IN ('310','311') THEN 1 ELSE 0 END),0) ENV
			FROM PC_CASE pc
			JOIN PC_JUDGEMENT pj ON pj.CASE_ID = pc.CASE_ID AND pj.REMOVED_BY = 0
			JOIN PC_LOOKUP_CASE_CATE plcc ON plcc.CASE_CATE_ID = pc.CASE_CATE_ID AND plcc.REMOVED_BY = 0
			WHERE pc.REMOVED_BY = 0
			AND TO_CHAR(pj.JUDGE_DATE,'DD-MM-YYYY') = $P{TODAY}) A_JUDGE]]>
		</queryString>
