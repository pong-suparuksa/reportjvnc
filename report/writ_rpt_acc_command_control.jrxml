	<queryString>
		<![CDATA[SELECT DISTINCT
	NVL(plc.COURT_NAME_TH,'-') COURT_NAME_TH,
    pn.PRINT_DATE,
    THAISDATE(pn.PRINT_DATE)              PRINT_DATE_TEXT,
    pn.NOTICE_DATE,
    THAISDATE(pn.NOTICE_DATE)             NOTICE_DATE_TEXT,
    pn.NOTICE_CODE_NO ||'/'|| NOTICE_CODE_YEAR	NOTICE_NO,
      CASE WHEN pc.BLACK_IDNUM IS NOT NULL 
		     THEN pltc_b.TITLE_CASE_NAME||pc.BLACK_IDNUM||'/'||pc.BLACK_YEAR 
		     ELSE '-' END CASE_BLACK_NO, 
    NVL(lit.LITIGANT_NAME,'-') DETAINEE,
    NVL(alle.ALLEGATION_NAME,'-') ALLEGATION_NAME,
    NVL(lit.PENALTY_NAME,'-') PENALTY_NAME,
    NVL(pn.NOTICE_REASON,'') NOTICE_REASON    
--
FROM PC_NOTICE pn 
LEFT JOIN PC_CASE pc ON pc.CASE_ID = pn.CASE_ID
--
LEFT JOIN PC_LOOKUP_NOTICE_TYPE plnt ON plnt.NOTICE_TYPE_ID = pn.NOTICE_TYPE_ID AND plnt.REMOVED_BY = 0
LEFT JOIN PC_LOOKUP_TITLE_CASE pltc_b ON pltc_b.COURT_ID = pc.COURT_ID AND pltc_b.TITLE_CASE_ID = pc.BLACK_TITLE_ID AND pltc_b.REMOVED_BY = 0
LEFT JOIN PC_LOOKUP_COURT plc ON plc.COURT_ID = pn.COURT_ID AND plc.REMOVED_BY = 0
--
LEFT JOIN (SELECT pca.CASE_ID,LISTAGG(pla.ALLEGATION_NAME, ', ') WITHIN GROUP (ORDER BY pla.ALLEGATION_NAME)  ALLEGATION_NAME
                   FROM PC_CASE_ALLE pca
                   JOIN PC_LOOKUP_ALLEGATION pla ON pla.ALLEGATION_ID = pca.ALLEGATION_ID AND pca.REMOVED_BY = 0
                   WHERE pca.REMOVED_BY = 0
                   GROUP BY pca.CASE_ID) alle ON alle.CASE_ID = pn.CASE_ID
--
LEFT JOIN (SELECT pcl.CASE_ID, pcl.LITIGANT_NAME,LISTAGG(plp.PENALTY_NAME, ', ') WITHIN GROUP (ORDER BY plp.PENALTY_NAME)  PENALTY_NAME
                   FROM PC_CASE_LIT pcl 
                   JOIN PC_LOOKUP_LIT_TYPE pllt ON pllt.LIT_TYPE_ID = pcl.LITIGANT_TYPE_ID AND pcl.REMOVED_BY = 0
                   LEFT JOIN PC_JUDGEMENT_ACCU_PENALTY pjap ON pjap.CASE_LIT_ID = pcl.CASE_LIT_ID AND pjap.REMOVED_BY = 0
                   LEFT JOIN PC_LOOKUP_PENALTY plp ON plp.PENALTY_ID = pjap.PENALTY_ID AND plp.REMOVED_BY = 0
                   WHERE pcl.REMOVED_BY = 0
                   AND pllt.LIT_TYPE_CODE = '03'
                   GROUP BY pcl.CASE_ID, pcl.LITIGANT_NAME) lit ON lit.CASE_ID = pn.CASE_ID
--           
WHERE pn.REMOVED_BY = 0
AND pc.CASE_ID IS NOT NULL
AND (pn.NOTICE_TYPE_CODE = $P{NOTICE_TYPE_CODE} OR $P{NOTICE_TYPE_CODE} IS NULL)
AND pn.NOTICE_DATE BETWEEN TO_DATE($P{START_DATE}||'T00:00:00', 'DD-MM-YYYY"T"HH24:MI:SS') 
                                              AND TO_DATE($P{END_DATE}||'T23:59:59', 'DD-MM-YYYY"T"HH24:MI:SS')
ORDER BY pn.NOTICE_DATE]]>
	</queryString>
