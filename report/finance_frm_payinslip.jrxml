	<queryString>
		<![CDATA[SELECT * 
FROM
(SELECT 
  NVL(plc.COURT_ID, 0)                    COURT_ID,
  NVL(plc.COURT_NAME_TH,'-')        COURT_NAME_TH,
  NVL(plc.TAX_ID,'-')                         TAX_ID,
  NVL(pfcc.COMP_CODE,'-')             COMP_CODE,
  NVL(pfi.CASE_ID, 0)                       CASE_ID,
  NVL(pfi.INVOICE_ID,0) 				    INVOICE_ID,
  NVL(pfi.PAYER_NAME,'-')               PAYER_NAME,
  NVL(pfi.REF1,'0')                            REF1,
  NVL(pfi.REF2,'0')                            REF2,
  TO_CHAR(pfi.DUE_DATE,'DD/MM/YYYY')          DUE_DATE,
  TO_CHAR(pfi.CREATED_DATE,'DD/MM/YYYY')  CREATED_DATE,
  NVL(pfi.TOTAL_PAID_AMOUNT,0)                      TOTAL_PAID_AMOUNT,
  BAHT(TOTAL_PAID_AMOUNT)                           TOTAL_PAID_AMOUNT_TEXT 
FROM PC_FIN_INVOICE pfi
LEFT JOIN PC_LOOKUP_COURT plc ON plc.COURT_ID = pfi.COURT_ID AND plc.REMOVED_BY = 0
CROSS JOIN PC_FIN_COMP_CODE pfcc
WHERE pfi.REMOVED_BY = 0
UNION
SELECT 
   1                                           COURT_ID,
  'สำนักงานศาลยุติธรรม'               COURT_NAME_TH,
  '1234567890123'                   TAX_ID,
  '909533'                                 COMP_CODE,
   19                                          CASE_ID,  
   1 										     INVOICE_ID,
  'นายปรีดีพงษ์ สมัครใจ'				 PAYER_NAME,
  '100162'                                  REF1,
  '0201200901050001'              REF2,
  '02/01/2019'                           DUE_DATE,
  '02/01/2562'                           CREATED_DATE,
  5000                                        TOTAL_PAID_AMOUNT,
  BAHT(5000)                             TOTAL_PAID_AMOUNT_TEXT  
FROM DUAL
) A
WHERE ROWNUM = 1
AND INVOICE_ID = $P{INVOICE_ID}]]>
	</queryString>
