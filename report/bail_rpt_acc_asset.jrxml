	<queryString language="SQL">
		<![CDATA[SELECT pltc.TITLE_CASE_NAME || pc.BLACK_IDNUM || '/' || pc.BLACK_YEAR  BLACK_CASE_NO,
     CASE WHEN pb.COURT_ORDER  = 1 THEN 'อนุญาต' ELSE 'ไม่อนุญาต' END COURT_ORDER_TEXT,
     pbb.NAME || ' ' ||pbb.LASTNAME BAILSMAN,
     NVL(pbs.TOTAL_AMOUNT ,0)   BAIL_COST,
     pc.PROS_DESC,
     pc.ACCU_DESC,
     pc.ALLE_DESC,
     plj.JUDGE_NAME,
     TO_CHAR(pb.BAIL_NOTES),
     CASE WHEN pbsa.SEC_ID > 0 THEN 'บัญชีประกันด้วยสมุดเงินฝาก' ELSE ' ' END BAIL_ACCOUNT,
     CASE WHEN pbsl.SEC_ID > 0 THEN 'บัญชีประกันด้วยที่ดิน' ELSE ' ' END BAIL_LAND,
     CASE 
	      WHEN pbsb.SEC_ID > 0 AND plbt.TYPE_NAME = 'พันธบัตร' THEN 'บัญชีประกันด้วยพันธบัตร'
          WHEN pbsb.SEC_ID > 0 AND plbt.TYPE_NAME = 'ฉลากออมสิน' THEN 'บัญชีประกันด้วยสลากออมทรัพย์'
          ElSE ' '
     END BAIL_BOND,
     CASE WHEN pbsbgt.SEC_ID > 0 THEN  'บัญชีประกันด้วยหลักทรัพย์หนังสือรับรองของธนาคาร' ELSE ' ' END BAIL_BANKGUARANTEE,
     CASE WHEN pbsi.SEC_ID > 0 THEN 'บัญชีประกันด้วยกรมธรรม์' ELSE ' ' END BAIL_INSURANCE,
     CASE WHEN pbscd.SEC_ID > 0 THEN 'บัญชีประกันด้วยห้องชุด' ELSE ' ' END BAIL_CONDO,
     CASE WHEN pbsp.SEC_ID > 0 THEN 'บัญชีประกันด้วยตำแหน่ง' ELSE ' ' END BAIL_POSITION,
     CASE WHEN pspc.SEC_ID > 0 THEN 'บัญชีประกันด้วยเงินสด' ELSE ' ' END BAIL_CASH,
     CASE WHEN pbsac.SEC_ID > 0 THEN 'หนังสือรับรองโดยต้นสังกัด' ELSE ' ' END BAIL_AFFILIATE_CERTIFICATE,
     LISTAGG(pla.ALLEGATION_NAME, ', ') WITHIN GROUP (ORDER BY pla.ALLEGATION_NAME) "ALLE"     
FROM PC_BAIL pb 
--
LEFT JOIN PC_BAIL_BAILSMAN pbb ON pbb.BAIL_ID = pb.BAIL_ID AND pbb.REMOVED_BY = 0
--
LEFT JOIN PC_CASE pc ON pc.CASE_ID = pb.CASE_ID AND pc.REMOVED_BY = 0
LEFT JOIN PC_LOOKUP_TITLE_CASE pltc ON pltc.TITLE_CASE_ID = pc.BLACK_TITLE_ID
LEFT JOIN PC_CASE_ALLE pca ON pca.CASE_ID = pc.CASE_ID 
--
LEFT JOIN PC_LOOKUP_JUDGE plj ON plj.JUDGE_ID = pb.JUDGE_ID AND plj.REMOVED_BY = 0
--
LEFT JOIN PC_BAIL_SEC pbs ON pbb.BAILSMAN_ID = pbs.BAILSMAN_ID AND pbs.REMOVED_BY = 0
LEFT JOIN PC_BAIL_SEC_ACCOUNT pbsa ON pbsa.SEC_ID = pbs.SEC_ID AND pbsa.REMOVED_BY = 0
LEFT JOIN PC_BAIL_SEC_LAND pbsl ON pbsl.SEC_ID = pbs.SEC_ID AND pbsl.REMOVED_BY = 0
LEFT JOIN PC_BAIL_SEC_BOND pbsb ON pbsb.SEC_ID = pbs.SEC_ID AND pbsb.REMOVED_BY = 0
LEFT JOIN PC_BAIL_SEC_BANKGUARANTEE pbsbgt ON pbsbgt.SEC_ID = pbs.SEC_ID AND pbsbgt.REMOVED_BY = 0
LEFT JOIN PC_BAIL_SEC_INSURANCE pbsi ON pbsi.SEC_ID = pbs.SEC_ID AND pbsi.REMOVED_BY = 0
LEFT JOIN PC_BAIL_SEC_CONDO pbscd ON pbscd.SEC_ID = pbs.SEC_ID AND pbscd.REMOVED_BY = 0
LEFT JOIN PC_BAIL_SEC_POSITION pbsp ON pbsp.SEC_ID = pbs.SEC_ID AND pbsp.REMOVED_BY = 0
LEFT JOIN PC_BAIL_SEC_CASH pspc ON pspc.SEC_ID = pbs.SEC_ID AND pspc.REMOVED_BY = 0
LEFT JOIN PC_BAIL_SEC_AFFILIATE_CERTIFICATE pbsac ON pbsac.SEC_ID = pbs.SEC_ID AND pbsac.REMOVED_BY = 0
LEFT JOIN PC_LOOKUP_BOND_TYPE plbt ON plbt.BOND_TYPE_ID=pbsb.BOND_TYPE_ID AND plbt.REMOVED_BY = 0
--
LEFT JOIN PC_LOOKUP_ALLEGATION pla ON pla.ALLEGATION_ID = pca.ALLEGATION_ID
WHERE pb.REMOVED_BY = 0 AND pb.BAIL_DATE = TO_DATE($P{SELECT_DATE}, 'DD-MM-YYYY') 
--
GROUP BY pltc.TITLE_CASE_NAME || pc.BLACK_IDNUM || '/' || pc.BLACK_YEAR,
       CASE WHEN pb.COURT_ORDER  = 1 THEN 'อนุญาต' ELSE 'ไม่อนุญาต' END,
       pbb.NAME || ' ' ||pbb.LASTNAME,
       NVL(pbs.TOTAL_AMOUNT,0),
       pc.PROS_DESC,
       pc.ACCU_DESC,
       pc.ALLE_DESC,
       plj.JUDGE_NAME,
       TO_CHAR(pb.BAIL_NOTES),
       CASE WHEN pbsa.SEC_ID > 0 THEN 'บัญชีประกันด้วยสมุดเงินฝาก' ELSE ' ' END,
       CASE WHEN pbsl.SEC_ID > 0 THEN 'บัญชีประกันด้วยที่ดิน' ELSE ' ' END,
       CASE WHEN pbsb.SEC_ID > 0 AND plbt.TYPE_NAME = 'พันธบัตร' THEN 'บัญชีประกันด้วยพันธบัตร' WHEN pbsb.SEC_ID > 0 AND plbt.TYPE_NAME = 'ฉลากออมสิน' THEN 'บัญชีประกันด้วยสลากออมทรัพย์' ElSE ' ' END,
       CASE WHEN pbsbgt.SEC_ID > 0 THEN  'บัญชีประกันด้วยหลักทรัพย์หนังสือรับรองของธนาคาร' ELSE ' ' END,
       CASE WHEN pbsi.SEC_ID > 0 THEN 'บัญชีประกันด้วยกรมธรรม์' ELSE ' ' END,
       CASE WHEN pbscd.SEC_ID > 0 THEN 'บัญชีประกันด้วยห้องชุด' ELSE ' ' END,
       CASE WHEN pbsp.SEC_ID > 0 THEN 'บัญชีประกันด้วยตำแหน่ง' ELSE ' ' END,
       CASE WHEN pspc.SEC_ID > 0 THEN 'บัญชีประกันด้วยเงินสด' ELSE ' ' END,
       CASE WHEN pbsac.SEC_ID > 0 THEN 'หนังสือรับรองโดยต้นสังกัด' ELSE ' ' END 
ORDER BY  CASE WHEN pb.COURT_ORDER  = 1 THEN 'อนุญาต' ELSE 'ไม่อนุญาต' END]]>
	</queryString>
