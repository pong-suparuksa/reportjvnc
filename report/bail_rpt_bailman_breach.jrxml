	<queryString language="SQL">
		<![CDATA[SELECT pltc.TITLE_CASE_NAME || pc.BLACK_IDNUM || '/' || pc.BLACK_YEAR  BLACK_CASE_NO,
plt.TITLE_NAME || pbb.NAME || ' ' || pbb.LASTNAME BAILMAN_FULL_NAME,
--
     CASE WHEN pbsa.SEC_ID > 0 THEN 'สมุดเงินฝาก' ELSE ' ' END BAIL_ACCOUNT,
     CASE WHEN pbsl.SEC_ID > 0 THEN 'ที่ดิน' ELSE ' ' END BAIL_LAND,
     CASE 
	      WHEN pbsb.SEC_ID > 0 AND pbsb.BOND_TYPE_ID = 1 THEN 'พันธบัตร'
          WHEN pbsb.SEC_ID > 0 AND pbsb.BOND_TYPE_ID = 2 THEN 'สลากออมทรัพย์'
          ElSE ' '
     END BAIL_BOND,
     CASE WHEN pbsbgt.SEC_ID > 0 THEN  'หลักทรัพย์หนังสือรับรองของธนาคาร' ELSE ' ' END BAIL_BANKGUARANTEE,
     CASE WHEN pbsi.SEC_ID > 0 THEN 'กรมธรรม์' ELSE ' ' END BAIL_INSURANCE,
     CASE WHEN pbscd.SEC_ID > 0 THEN 'ห้องชุด' ELSE ' ' END BAIL_CONDO,
     CASE WHEN pbsp.SEC_ID > 0 THEN 'ตำแหน่ง' ELSE ' ' END BAIL_POSITION,
     CASE WHEN pspc.SEC_ID > 0 THEN 'เงินสด' ELSE ' ' END BAIL_CASH,
--
THAISDATE(pbbb.BREACH_START_DATE) BREACH_START_DATE,
pbbb.AMOUNT AMOUNT,
pbbb.BREACH_DESCRIPTION BREACH_DESCRIPTION
FROM PC_BAIL_BAILSMAN_BREACH pbbb
--
LEFT JOIN PC_BAIL pb ON pb.BAIL_ID = pbbb.BAIL_ID AND pb.REMOVED_BY = 0
LEFT JOIN PC_BAIL_BAILSMAN pbb ON pbb.BAILSMAN_ID = pbbb.BAILSMAN_ID AND pbb.REMOVED_BY = 0
LEFT JOIN PC_LOOKUP_TITLE plt ON plt.TITLE_ID = pbb.NAME_TITLE_ID AND plt.REMOVED_BY = 0
--
LEFT JOIN PC_CASE pc ON pc.CASE_ID = pb.CASE_ID AND pc.REMOVED_BY = 0
LEFT JOIN PC_LOOKUP_TITLE_CASE pltc ON pltc.TITLE_CASE_ID = pc.BLACK_TITLE_ID AND pltc.REMOVED_BY = 0
--
LEFT JOIN PC_BAIL_SEC_ACCOUNT pbsa ON pbsa.SEC_ID = pbbb.SEC_ID AND pbsa.REMOVED_BY = 0
LEFT JOIN PC_BAIL_SEC_LAND pbsl ON pbsl.SEC_ID = pbbb.SEC_ID AND pbsl.REMOVED_BY = 0
LEFT JOIN PC_BAIL_SEC_BOND pbsb ON pbsb.SEC_ID = pbbb.SEC_ID AND pbsb.REMOVED_BY = 0
LEFT JOIN PC_BAIL_SEC_BANKGUARANTEE pbsbgt ON pbsbgt.SEC_ID = pbbb.SEC_ID AND pbsbgt.REMOVED_BY = 0
LEFT JOIN PC_BAIL_SEC_INSURANCE pbsi ON pbsi.SEC_ID = pbbb.SEC_ID AND pbsi.REMOVED_BY = 0
LEFT JOIN PC_BAIL_SEC_CONDO pbscd ON pbscd.SEC_ID = pbbb.SEC_ID AND pbscd.REMOVED_BY = 0
LEFT JOIN PC_BAIL_SEC_POSITION pbsp ON pbsp.SEC_ID = pbbb.SEC_ID AND pbsp.REMOVED_BY = 0
LEFT JOIN PC_BAIL_SEC_CASH pspc ON pspc.SEC_ID = pbbb.SEC_ID AND pspc.REMOVED_BY = 0
--
WHERE pbbb.BREACH_FINISH_DATE IS NULL
AND pbbb.BREACH_START_DATE BETWEEN TO_DATE($P{START_DATE}||'T00:00:00', 'DD-MM-YYYY"T"HH24:MI:SS') 
                               AND TO_DATE($P{END_DATE}||'T23:59:59', 'DD-MM-YYYY"T"HH24:MI:SS')]]>
	</queryString>
