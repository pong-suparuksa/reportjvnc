	<parameter name="BAIL_ID" class="java.lang.Integer">
		<defaultValueExpression><![CDATA[1]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[SELECT DISTINCT
THAILDATE(pb.BAIL_DATE), dbms_lob.substr( pb.BAIL_NOTES, 4000, 1) BAIL_NOTES,
pltc.TITLE_CASE_NAME || pc.BLACK_IDNUM || '/' || pc.BLACK_YEAR  BLACK_CASE_NO,
pltc_red.TITLE_CASE_NAME || pj.RED_IDNUM || '/' || pj.RED_YEAR  RED_CASE_NO,
plgt.GUAR_TITLE_NAME || pb.BAIL_CODE_NO || '/' || pb.BAIL_CODE_YEAR BAIL_CASE_NO,
plt.TITLE_NAME || pbb.NAME || ' ' || pbb.LASTNAME BAILMAN_NAME,
plc.COURT_NAME_TH, pc.PROS_DESC, pc.ACCU_DESC, pc.ALLE_DESC

FROM PC_BAIL_SEC pbs
--
LEFT JOIN PC_BAIL_SEC_TYPE pbst ON pbst.SEC_TYPE_ID = pbs.SEC_TYPE_ID AND pbst.REMOVED_BY = 0
LEFT JOIN PC_BAIL pb ON pb.BAIL_ID = pbs.BAIL_ID AND pb.REMOVED_BY = 0
LEFT JOIN PC_BAIL_DETAIL pbd ON pbd.BAIL_ID = pb.BAIL_ID AND pbd.REMOVED_BY = 0
LEFT JOIN PC_LOOKUP_GUAR_TITLE plgt ON plgt.GUAR_TITLE_ID = pb.BAIL_TITLE_ID AND plgt.REMOVED_BY = 0
LEFT JOIN PC_LOOKUP_COURT plc ON plc.COURT_ID = pb.COURT_ID AND plc.REMOVED_BY = 0
--
LEFT JOIN PC_BAIL_SEC_BAILSMAN pbsb ON pbsb.SEC_ID = pbs.SEC_ID AND pbsb.REMOVED_BY = 0
LEFT JOIN PC_BAIL_BAILSMAN pbb ON pbb.BAILSMAN_ID = pbsb.BAILSMAN_ID AND pbb.REMOVED_BY = 0
LEFT JOIN PC_LOOKUP_TITLE plt ON plt.TITLE_ID = pbb.NAME_TITLE_ID AND plt.REMOVED_BY = 0
--
LEFT JOIN PC_CASE pc ON pc.CASE_ID = pb.CASE_ID AND pc.REMOVED_BY = 0
LEFT JOIN PC_LOOKUP_TITLE_CASE pltc ON pltc.TITLE_CASE_ID = pc.BLACK_TITLE_ID AND pltc.REMOVED_BY = 0
LEFT JOIN PC_JUDGEMENT pj ON pj.CASE_ID = pc.CASE_ID AND pj.REMOVED_BY = 0
LEFT JOIN PC_LOOKUP_TITLE_CASE pltc_red ON pltc_red.TITLE_CASE_ID = pj.RED_TITLE_ID AND pltc_red.REMOVED_BY = 0
--
WHERE pb.bail_id = $P{BAIL_ID} AND pc.REMOVED_BY = 0]]>
	</queryString>
