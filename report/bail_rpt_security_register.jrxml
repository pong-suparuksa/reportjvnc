	<parameter name="SELECT_DATE" class="java.lang.String">
		<defaultValueExpression><![CDATA["10/03/2020"]]></defaultValueExpression>
	</parameter>
	<queryString language="SQL">
		<![CDATA[SELECT BD.BAIL_DATE,
LTC.TITLE_CASE_NAME || C.BLACK_IDNUM || '/' || C.BLACK_YEAR  BLACK_CASE_NO, C.ALLE_DESC ALLE,
LT.TITLE_NAME || BB.NAME || ' ' || BB.LASTNAME BAILMAN_NAME,
THAISDATE(BD.BAIL_DATE) THAIDATE,	
CASE BSP.POSITION_TYPE
	WHEN 1 THEN 'ข้าราชการ'
    WHEN 2 THEN 'รัฐวิสาหกิจ'
    ELSE 'อื่น ๆ'
END,
BSP.POSITION, BS.TOTAL_AMOUNT DEFENDANT_BAIL_COST , LC.COURT_NAME_TH,
CASE BD.COURT_ORDER
	WHEN 1 THEN 'อนุญาต'
    ELSE 'ไม่อนุญาต'
END
FROM PC_BAIL_SEC_POSITION BSP
LEFT JOIN PC_BAIL_SEC BS ON(BS.SEC_ID=BSP.SEC_ID)
LEFT JOIN PC_BAIL BD ON(BD.BAIL_ID=BS.BAIL_ID)
LEFT JOIN PC_CASE C ON(C.CASE_ID=BD.CASE_ID)
LEFT JOIN PC_LOOKUP_TITLE_CASE LTC ON(LTC.TITLE_CASE_ID=C.BLACK_TITLE_ID)
LEFT JOIN PC_LOOKUP_COURT LC ON(LC.COURT_ID=BD.COURT_ID)
LEFT JOIN PC_BAIL_BAILSMAN BB ON(BB.BAILSMAN_ID=BS.BAILSMAN_ID)
LEFT JOIN PC_LOOKUP_TITLE LT ON(LT.TITLE_ID=BB.NAME_TITLE_ID)
WHERE BD.BAIL_DATE = TO_DATE($P{SELECT_DATE}, 'DD-MM-YYYY')]]>
	</queryString>
